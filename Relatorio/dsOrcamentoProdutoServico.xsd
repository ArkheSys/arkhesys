          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="dsOrcamentoProdutoServicoMaterialTableAdapter" GeneratorDataComponentClassName="dsOrcamentoProdutoServicoMaterialTableAdapter" Name="dsOrcamentoProdutoServicoMaterial" UserDataComponentName="dsOrcamentoProdutoServicoMaterialTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="cwkcwkGestaoConnectionString (Settings)" DbObjectName="" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>--Relativo a Material (Material de Uso e Consumo)
select row_number() over (order by x.ProdutoNome) Contador,
	   x.ProdutoNome,
	   x.Unidade,
	   x.Quantidade,
	   x.ValorCalculado, 
	   x.Total
from (select pit.ProdutoNome,
		pit.Unidade,
		pit.Quantidade,
		pit.ValorCalculado, 
		pit.Total
from Pedido p
join TipoPedido tp on tp.ID = p.IDTipoPedido
join PedidoItem pit on pit.IDPedido = p.ID
join Produto pr on pr.ID = pit.IDProduto
join GrupoEstoque ge on ge.ID = pr.IDGrupoEstoque
where p.ID = @IDPedido
AND	ge.ClassificacaoProduto = 8
group by tp.Categoria, pit.ProdutoNome, pit.Unidade, pit.Quantidade, 
	     pit.ValorCalculado, pit.Total, ge.Classificacao,ge.ClassificacaoProduto) x
</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="IDPedido" ColumnName="" DataSourceName="" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@IDPedido" Precision="0" Scale="0" Size="4" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
                    </Parameters>
                  </DbCommand>
