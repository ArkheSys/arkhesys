            <MainSource>
              <DbSource ConnectionRef="cwkGestaoConnectionString (Settings)" DbObjectName="SELECT plp.IDPLPWS AS [NumeroPLP], f.correioCartaoPostagem AS 'CartãoPostagem', plp.ID AS 'IdPLP', tt.CodigoServico AS 'Serviço', COUNT(rc.Numero) AS 'Quantidade' FROM PLP AS plp INNER JOIN PLPDetalhe AS plpd ON plpd.IDPLP = plp.ID INNER JOIN pedido AS p ON p.id = plpd.IDPedido INNER JOIN CorreiosPrecoPrazo AS cpp ON cpp.IDPedido = p.ID INNER JOIN RastreamentoCorreio AS rc ON cpp.IDRastreamento = rc.ID AND rc.SolicitadoWScorreios = 1 INNER JOIN TipoServicoTransporte tt ON RC.IDTipoServico = tt.ID AND tt.Correios = 1 INNER JOIN Filial F ON f.ID = p.IDFilial GROUP BY f.correioCartaoPostagem, tt.CodigoServico, plp.IDPLPWS, plp.ID" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select t.*
  from ( 
select plp.IDPLPWS as "NumeroPLP",
       f.correioCartaoPostagem as 'CartãoPostagem',
       plp.ID as 'IdPLP',
	   tt.CodigoServico as 'Serviço',
       COUNT(rc.Numero) as 'Quantidade'
  from PLP as plp
 inner join PLPDetalhe as plpd on plpd.IDPLP = plp.ID
 inner join pedido as p on p.id = plpd.IDPedido
 inner join CorreiosPrecoPrazo as cpp on cpp.IDPedido = p.ID
 inner join RastreamentoCorreio as rc on cpp.IDRastreamento = rc.ID and rc.SolicitadoWScorreios = 1
 INNER JOIN TipoServicoTransporte tt on RC.IDTipoServico = tt.ID and tt.Correios = 1
 INNER JOIN Filial F on f.ID = p.IDFilial
 group by f.correioCartaoPostagem,
	      tt.CodigoServico,
	      plp.IDPLPWS,
          plp.ID
         ) t</CommandText>
