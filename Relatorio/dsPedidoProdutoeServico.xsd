              <DbSource ConnectionRef="cwkcwkGestaoConnectionString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>
						select
						f.fantasia as EmpresaNome,
						f.CNPJ as EmpresaCNPJ,
						f.Inscricao as EmpresaInscricao,
						f.Endereco + ' ' + f.Numero as EmpresaEndereco,
						f.Bairro as EmpresaBairro,
						c.Nome as EmpresaCidade,
						u.Sigla as EmpresaUF,
						f.CEP as EmpresaCEP,
						f.Telefone as EmpresaTelefone,
						case tp.tipo
						when '1' then 'Orçamento'
						else 'Pedido'
						end
						tipopedido,
						f.Email as EmpresaEmail,
						pd.Numero as Pedidonumero,
						CONVERT(varchar,pd.IncData, 103) as DataEmissao,
						CONVERT(varchar,pd.DtPrazoEntrega, 103) as DtPrevEntrega,
						pd.Validade as pedidoValidade,
						pv.nome as pedidoVendedorNome,
						case p.TipoPessoa
						when 'Jurídica' then p.Inscricao
						else p.RG
						end
						pedidoPessoaInscricao,
						p.Nome as pedidoPessoaNome,
						case p.TipoPessoa
						when 'Jurídica' then p.Fantasia
						else p.nome
						end
						pedidoPessoaApelido,
						pe.Endereco as pedidoPessoaEndereco,
						pe.Numero as pedidoPessoaEnderecoNumero,
						pe.Bairro as pedidoPessoaEnderecoBairro,
						cp.Nome + ' - ' + up.Sigla as pedidoPessoaEnderecoCidade,
						pe.CEP as pedidoPessoaEnderecoCEP,
						p.CNPJ_CPF as pedidoPessoaCNPJ_CPF,
						pte.Numero as pedidoPessoaTelefone,
						pem.Email as pedidoPessoaEmail,
						pee.Endereco as pedidoEnderecoEntrega,
						pee.Numero as pedidoNumeroEntrega,
						pee.Bairro as pedidoBairroEntrega,
						ce.Nome + ' - ' + ue.Sigla as pedidoCidadeEntrega,
						pee.Telefone as pedidoTelefoneEntrega,
						pt.Nome as pedidoPessoaTransportadora,
						cd.Nome as pedidoCondicao,
						pr.Codigo as ProdutoCodigo,
						pr.Nome as ProdutoDescricao,
						un.Sigla as ProdutoUN,
						pip.Quantidade as ProdutoQuantidade,
						convert(decimal(18,2),pip.Valor) as ProdutoValorUn,
						pip.Total as ProdutoValorTotal,
						pd.TotalProduto as PedidoTotalProduto,

						ser.codigo as ServicoCodigo,
						pis.ServicoNome as ServicoDescricao,
						un.Sigla as ServicoUN,
						pis.Quantidade as ServicoQuantidade,
						convert(decimal(18,2),pis.Valor) as ServicoValorUn,
						pis.Total as ServicoValorTotal,
						pd.TotalProduto as PedidoTotalProduto,
						pd.TotalServico as PedidoTotalServico,
						pd.ValorFrete as PedidoValorFrete,
						pd.TotalPedido as PedidoTotal,
						(pd.TotalST + pd.TotalIPI) as PedidoTotalTributos
						from Pedido pd
						left join Filial f on f.ID = pd.IDFilial
						left join Cidade c on c.ID = f.IDCidade
						left join UF u on u.ID = c.IDUF
						left join Pessoa p on p.id = pd.IDPessoa
						left join Condicao cd on cd.ID = pd.IDCondicao
						left join PedidoItem pip on pip.IDPedido = pd.ID
						left join PedidoItemServico pis on pis.IDPedido = pd.ID
						LEFT JOIN PessoaEndereco as pe ON pe.ID =  (SELECT TOP (1) ID FROM PessoaEndereco WHERE((IDPessoa = pd.IDpessoa) AND (bprincipal = 1)) order by bprincipal desc)
						LEFT JOIN PessoaTelefone as pte ON pte.ID = (SELECT TOP (1) ID FROM PessoaTelefone WHERE(IDPessoa = p.ID) AND (bPrincipal = 1))
						LEFT JOIN PessoaEmail as pem ON pem.ID = (SELECT TOP (1) ID FROM PessoaEmail WHERE(IDPessoa = p.ID) AND (bCompra = 1))
						left join cidade cp on cp.ID = pe.IDCidade
						left join UF up on up.id = cp.IDUF
						left join TipoPedido tp on tp.ID = pd.IDTipoPedido
						left join Produto pr on pr.id = pip.IDProduto
						left join Servico ser on ser.id = pis.IDServico
						left join Unidade un on un.ID = pr.IDUnidade
						left join PessoaVendedor pva on (pd.idVendedor = pva.ID)
						left join Pessoa pv on pv.ID = pva.IDPessoa
						left join Pessoa pt on pt.ID = pd.IDPessoaTransportadora
						left join PessoaEndereco pee on pee.ID = pd.IDEnderecoEntrega
						left join Cidade ce on ce.ID = pee.IDCidade
						left join UF ue on ue.id = ce.IDUF

						WHERE pd.ID = @pPedido</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="pPedido" ColumnName="ID" DataSourceName="DEMOGESTAO.dbo.Pedido" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@pPedido" Precision="0" ProviderType="Int" Scale="0" Size="4" SourceColumn="ID" SourceColumnNullMapping="false" SourceVersion="Current" />
                    </Parameters>
                  </DbCommand>
                </SelectCommand>
