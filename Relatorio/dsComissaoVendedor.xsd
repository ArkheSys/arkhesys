        <Tables>
          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="ComissaoVendedorPedidoTableAdapter" GeneratorDataComponentClassName="ComissaoVendedorPedidoTableAdapter" Name="ComissaoVendedorPedido" UserDataComponentName="ComissaoVendedorPedidoTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="cwkGestaoConnectionString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select 
		n.id, n.Codigo, n.Dt, n.PessoaNome, n.TotalNota as ValorNota, n.ValorDesconto,
		coalesce (pe1.Nome, pe2.Nome, '(Nenhum)') as Vendedor, n.TotalNota, p.TotalPedido,
		case
			when cast((select conf.ComissaoPedido from Configuracao conf) as int) = 1 then 
				coalesce (p.ValorComissao, coalesce(pv1.ComissaoVendedor, pv2.ComissaoVendedor, 0.0) * p.TotalPedido / 100)
			else 
				coalesce(pv1.ComissaoVendedor, pv2.ComissaoVendedor, 0.0) * p.TotalPedido / 100
		end as ValorComissao,
		case
			when cast((select conf.ComissaoPedido from Configuracao conf) as int) = 1 then
				coalesce (p.PercComissao, pv1.ComissaoVendedor, pv2.ComissaoVendedor, 0.0)
			else
				coalesce (pv1.ComissaoVendedor, pv2.ComissaoVendedor, 0.0)
		end as PctComissao,
		
		prod.Nome as Produto, pedi.ValorCalculado
		
from Nota n
left join NotaItem ni on ni.IDNota = n.ID
left join NotaItemPedidoItem nipi on nipi.IDNotaItem = ni.ID
left join PedidoItem pedi on nipi.IDPedidoItem = pedi.ID
left join Produto prod on ni.IDProduto = prod.ID
left join Pedido p on p.ID = pedi.IDPedido
left join Pessoa pe1 on pe1.ID = p.IDVendedor
left join Pessoa pe2 on pe2.ID = p.IDVendedor2
left join PessoaVendedor pv1 on pe1.ID = pv1.IDPessoa
left join PessoaVendedor pv2 on pe2.ID = pv2.IDPessoa
left join TipoNota tn on n.IDTipoNota = tn.ID

where tn.Ent_Sai = 2</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
            </MainSource>
