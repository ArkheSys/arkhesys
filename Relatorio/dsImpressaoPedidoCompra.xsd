                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select *, 
		(ordem - 1)/8 as Grupo
 from (
select
		
			f.Nome as EmpresaNome,
			f.CNPJ as EmpresaCNPJ,
			f.Inscricao as EmpresaInscricao,
			f.Endereco + ' ' + f.Numero as EmpresaEndereco,
			f.Bairro as EmpresaBairro, 
			f.EmailRecebimentoNFe as EmpresaEmailNFe, 
			f.Email as EmpresaEmailfinanceiro,		                     
			c.Nome as EmpresaCidade,
			uf.Sigla as EmpresaUF,
			f.CEP as EmpresaCEP,
			f.Telefone as EmpresaTelefone,
			f.Email as EmpresaEmail,
			p.Numero as PedidoNumero,
			p.IncUsuario as Comprador,
			convert(varchar,p.Dt,103) as DataEmissao,
			coalesce(pend.contato, '') as VendedorNome,
			(SELECT TOP (1) Email FROM PessoaEmail WHERE (idpessoa = p.IDpessoa and bVenda = 1)) as VendedorEmail,
			p.PessoaNome as FornecedorNome,
			pe.fantasia as FornecedorFantasia,
			p.PessoaEndereco as FornecedorEndereco,
			(SELECT TOP (1) Complemento from PessoaEndereco where (pend.IDPessoa = p.IDPessoa and bPrincipal = 1)) as FornecedorComplemento,
			p.PessoaCEP as FornecedorCEP,
			p.PessoaCidade as FornecedorCidade,
			p.PessoaBairro as FornecedorBairro,
			p.PessoaUF as FornecedorUF,
			p.PessoaCNPJCPF as FornecedorCNPJ,
			p.PessoaInscRG as FornecedorInscricao,
			p.PessoaTelefone as FornecedorTelefone,
			con.Nome as CondicaoNome,
			CAST(p.TipoFrete as varchar(50)) as TipoFreteValor,
			case
			when p.TipoFrete = '0' then (
				'( X ) CIF  (   ) FOB')
			when p.TipoFrete = '1' then (
				'(   ) CIF  ( X ) FOB')
			when p.TipoFrete = '2' then (
				' 2- Terceiros' )
			when p.TipoFrete = '3' then (
				' 3- Sem Frete' )
			end	
			as TipoFrete,
		
		
			p.TransNome as TransportadoraNome,
			p.ValorFrete as FreteValor,
			p.TotalProduto + p.ValorFrete as SubTotal,
			p.ValorDesconto as TotalDesconto,
			p.TotalIPI as TotalIPI,
			p.TotalPedido as TotalPedido,
			p.Observacao1 as Observacao1,
			p.Observacao2 as Observacao2,
			p.Observacao3 as Observacao3,
		
			pi.Sequencia as ProdutoSequencia,
			prod.Nome as ProdutoNome,
			pi.AplicacaoCompra as ProdutoAplicacao,
			pi.Quantidade as ProdutoQuantidade,
			un.Sigla as ProdutoUnidade,
			prod.NCM as ProdutoNCM,
			prod.Codigo as ProdutoCodigo,
			pi.Valor as ProdutoValor,
			pi.Total as ProdutoValorTotal,
			pi.ValorDesconto as ProdutoValorDesconto,
			pi.ValorCalculado as ProdutoValorCalculado,
			pi.IPICompra as ProdutoIPI,
			pi.ICMSCompra as ProdutoICMS,
			pi.InformacoesAdicionais as ProdutoInfAdicionais,
			

			ROW_NUMBER() over(order by pi.sequencia)  as Ordem

	from Pedido p
	left join Filial f on f.id = p.IDFilial
	left join Cidade c on c.ID = f.IDCidade
	left join UF uf on uf.ID = c.IDUF
	left join Pessoa pe on pe.ID = p.IDPessoa
	left join PessoaEmail pem on pem.IDPessoa = p.IDPessoa
	left join Condicao con on con.id = p.IDCondicao
	left join PedidoItem pi on pi.IDPedido = p.ID
	left join Produto prod on prod.ID = pi.IDProduto
	left join Unidade un on un.ID = prod.IDUnidade
	left join PessoaEndereco pend on pend.IDPessoa = p.IDPessoa
	

	where p.id = 821
	)t

</CommandText>
