            <MainSource>
              <DbSource ConnectionRef="cwkcwkGestaoConnectionString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select tg.Nome as 'TelGrupo',
	   ts.Descricao as 'Serviço',
	   tf.Codigo as 'Código Fatura',
	   tf.Vencimento as 'Vencimento',
	   p.Nome as 'Cliente',
	   p.Fantasia as 'Fantasia',
	   sum(tfs.Total) as 'Valor Servico'
from Tel_Fatura tf
inner join Tel_Cliente tc on tc.Id = tf.IdTel_Cliente
inner join Tel_GrupoCliente tg on tg.Id = tc.IdTel_GrupoCliente
inner join Tel_FaturaServico tfs on tfs.IdTel_Fatura = tf.ID
inner join Tel_Servico ts on ts.ID = tfs.IdTel_Servico
inner join Pessoa p on p.ID = tc.IdPessoa
where 	
	tg.Id in (select * from F_RetornaTabelaLista(@in_GrupoCliente, ',')) and
	ts.Id in (select * from F_RetornaTabelaLista(@in_Servico, ',')) and
	DATEPART (MONTH ,tf.Referencia) = @RefMes and
	DATEPART (YEAR ,tf.Referencia) = @RefAno
group by ts.Descricao, tg.Nome, tf.Vencimento, tf.Referencia, p.Nome, p.Fantasia, tf.Codigo </CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="RefMes" ColumnName="" DataSourceName="" DataTypeServer="unknown" DbType="Decimal" Direction="Input" ParameterName="@RefMes" Precision="0" Scale="0" Size="0" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
