                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        p.Codigo AS 'Prod Código', p.Nome AS 'Matéria Prima', ftItem.Quantidade, ftItem.Aproveitamento, ftItem.IdFichaTecnicaPCPFilha AS 'Filha', u.Sigla AS 'Unidade', 
                         ftPCP.Nome, CONVERT(VARCHAR(10), ftItem.DataVigor, 103) AS 'Data', CASE WHEN ftItem.IdFichaTecnicaPCPFilha IS NULL THEN 0 ELSE 1 END AS bit, 
                         ftPCP.Codigo, ftPCP.Id as 'FT id'
FROM            PCP_ItemFichaTecnica AS ftItem INNER JOIN
                         Produto AS p ON p.ID = ftItem.IdProduto INNER JOIN
                         Unidade AS u ON p.IDUnidade = u.ID INNER JOIN
                        PCP_FichaTecnica AS ftPCP ON ftPCP.Id = ftItem.IdFichaTecnicaPCP</CommandText>
