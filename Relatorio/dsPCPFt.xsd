                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT fl.nome                AS 'Emp Nome', 
       fl.cnpj                AS 'Emp CNPJ', 
       fl.endereco + ', ' + fl.Numero + ', '  + fl.Bairro +  ' - ' + fl.[Nome Cidade] + ' - ' + fl.Sigla + ' - CEP: ' + fl.CEP   AS 'Emp Endereço', 
       fl.telefone            AS 'Emp Telefone', 
       ftp.codigo             AS 'FT Código', 
       ftp.pecasop            AS 'FT Pecas OP', 
       ftp.barra              AS 'FT Código de Barras', 
       ftp.id                 AS 'FT id', 
       ftp.numerorevisao      AS 'FT Numero Revisão', 
       ftp.nome               AS 'FT Versão', 
       ftp.Data				  AS 'FT Data',
       ftp.comprimentobruto   AS 'FT Compr Bruto', 
       ftp.comprimentoliquido AS 'FT Compr Liquido', 
       ftp.largurabruta       AS 'FT Largura Bruta', 
       ftp.larguraliquida     AS 'FT Largura Liquida', 
       ftp.espessurabruta     AS 'FT Espessura Bruta', 
       ftp.espessuraliquida   AS 'FT Espessura Liquida', 
       ftp.areabruta          AS 'FT Area Bruta', 
       ftp.arealiquida        AS 'FT Area Liquida', 
       ftp.volumebruto        AS 'FT Volume Bruto', 
       ftp.volumeliquido      AS 'FT Volume Liquido', 
       ftp.pesobruto          AS 'FT Peso Bruto', 
       ftp.pesoliquido        AS 'FT Peso Liquido', 
       ftp.imagem             AS 'FT Imagem Produto', 
       ftp.observacao         AS 'FT Observações', 
       p.desclonga1           AS 'Prod Descrição', 
       ftp.formula 
  FROM (SELECT TOP (1) f.id, 
                       f.codigo, 
                       f.nome, 
                       c.Nome as 'Nome Cidade',
                       cnpj, 
                       inscricao, 
                       endereco, 
                       bairro, 
                       idcidade, 
                       telefone, 
                       fax, 
                       filialnf, 
                       idempresa, 
                       tipo, 
                       numero, 
                       responsavel, 
                       cep, 
                       certificado, 
                       usuarioemail, 
                       senhaemail, 
                       servidorsmtp, 
                       tipocertificadonfe, 
                       modelodanfe, 
                       formaemissao, 
                       ambiente, 
                       tipost, 
                       simplesnacional, 
                       aliqsimplessubst, 
                       idtabelafaixafatsimples, 
                       email, 
                       site, 
                       benviarcopiaemail, 
                       emailnfe, 
                       horascan, 
                       justificativascan, 
                       seriescan, 
                       portasmtp, 
                       versaoesquema, 
                       diretoriopadraonfe, 
                       pinnfe, 
                       gmailautenticacao, 
                       cnae, 
                       u.Sigla,
                       inscricaomunicipal, 
                       incentivadorcultural, 
                       naturezatributacao, 
                       grupomanager, 
                       diretoriomonitoramento, 
                       regimeespecialtributacao, 
                       bstsomenterevenda, 
                       ncdempresa, 
                       sdssenha 
          FROM filial AS f 
          INNER JOIN Cidade c ON c.ID = f.IDCidade
          INNER JOIN UF u ON u.ID = c.IDUF
         ORDER BY f.id) AS fl 
       CROSS JOIN pcp_fichatecnica AS ftp 
       
       INNER JOIN produto AS p 
               ON p.id = ftp.idproduto               
       WHERE ftp.Id IN (SELECT * FROM F_RetornaTabelaLista(@IdFT, ','))       </CommandText>
