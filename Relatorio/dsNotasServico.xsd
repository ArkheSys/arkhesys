                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT n.Serie AS 'NotaSerie',
       n.Numero AS 'NotaNumero',
       n.Dt AS 'NotaData',
       p.Nome AS 'ClienteNome',
       n.TotalNota AS 'NotaValor',
	   tn.Nome AS 'TipoNotaNome',
	   s.nome AS 'ServicoNome',
	   tn.ID
FROM Nota n
INNER JOIN Pessoa p ON p.ID = n.IDPessoa
INNER JOIN TipoNota tn ON tn.ID = n.IDTipoNota
INNER JOIN NotaItemServico nsi ON nsi.IDNota = n.ID
INNER JOIN Servico s on S.id = nsi.IDServico
WHERE
n.Dt between @DtInicio and @DtFinal
AND n.IDFilial in (select * from F_RetornaTabelaLista(@inFilial, ','))
AND n.IDTipoNota in (select * from F_RetornaTabelaLista(@inTipoNota, ','))
AND nsi.IDServico in (select * from F_RetornaTabelaLista(@inServico, ','))</CommandText>
