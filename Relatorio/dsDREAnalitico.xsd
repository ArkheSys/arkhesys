            <MainSource>
              <DbSource ConnectionRef="FenixDRE2ConnectionString (Settings)" DbObjectName="SELECT dre.*, CASE WHEN dre.TipoConta = 'R' THEN 1 WHEN dre.TipoConta = 'C' THEN 3 WHEN dre.TipoConta = 'E' THEN 4 ELSE 2 END ordem FROM F_dresintetico(@DATAINI,@DATAFIN, @IdFilial) AS dre WHERE ((tipo = 'R' AND TipoConta = 'E') OR (tipo = 'V' AND TipoConta IN ('R','D','C')))" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select t.TipoConta,
       t.ordem,
	   case when t.TipoConta = 'R' then
				'Total Receitas:' 
		    when t.TipoConta = 'C' then
				'Total Custos:' 
			when t.TipoConta = 'E' then
			    'Resultado do Exercício:'
			Else 'Total Despesas:' 
	   End DescTipoConta,
	   SUM(t.valor) valor
  from (
	 select dre.*,
			case when dre.TipoConta = 'R' then
				1
		    when dre.TipoConta = 'C' then
				3
			when dre.TipoConta = 'E' then
			    4
			Else 2 end ordem
	   FROM F_dresintetico(@DATAINI,@DATAFIN, @IdFilial) as dre 
	  where ((tipo = 'R' and TipoConta = 'E') or (tipo = 'V' and TipoConta in ('R','D','C')))
	   ) t
	group by t.TipoConta, t.ordem  </CommandText>
