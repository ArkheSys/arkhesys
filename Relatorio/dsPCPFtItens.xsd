                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT p.codigo                                    AS 'ProdCodigo', 
       p.nome                                      AS 'MateriaPrima', 
       ftItem.quantidade, 
       ftItem.aproveitamento, 
       ftItem.IdFichaTecnicaPCP                    AS 'idFT', 
       CONVERT(VARCHAR(10), ftItem.datavigor, 103) AS 'Data', 
       CASE 
         WHEN ftItem.idfichatecnicapcpfilha IS NULL THEN 0 
         ELSE 1 
       END                                         AS bit, 
       ftItem.idfichatecnicapcpfilha               AS 'Filha',
       u.sigla                                     AS 'Unidade',
       ftPCP.codigo, 
       ftPCP.nome
  FROM PCP_ItemFichaTecnica AS ftItem 
       INNER JOIN produto AS p 
               ON p.id = ftItem.idproduto 
       INNER JOIN unidade AS u 
               ON p.idunidade = u.id 
       LEFT JOIN PCP_FichaTecnica AS ftPCP 
               ON ftPCP.id = ftItem.IdFichaTecnicaPCPFilha</CommandText>
