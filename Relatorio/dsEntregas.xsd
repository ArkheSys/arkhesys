                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>--Entregas Efetuadas

select
	pi.ID,
	ent.ID as IDEntrega,
	pd.ID as CodigoPedido,
	Convert(varchar, ent.Data, 103) as DataEntrega,
	Convert(varchar, ent.Data, 14) as HoraEntrega,
	f.fantasia as FilialNome,
	f.CNPJ as FilialCNPJ,
	f.Inscricao as FilialInscricao,
	f.Endereco + ' ' + f.Numero as FilialEndereco,
	f.Bairro as FilialBairro,
	c.Nome as FilialCidade,
	u.Sigla as FilialUF,
	f.CEP as FilialCEP,
	f.Telefone as FilialTelefone,
	f.Email as FilialEmail,
	case tp.tipo 
	when '1' then 'Entrega: Orçamento'
	else 'Entrega: Pedido'
	end
	tipopedido,
	f.Email as EmpresaEmail,
	pd.Numero as Pedidonumero,
	CONVERT(varchar, pd.Dt, 103) as DataEmissao,
	CONVERT(varchar, pd.Dt, 14) as HoraEmissao,
    CONVERT(varchar,(pd.Dt + pd.Validade), 103) as pedidoValidade,
    pv.nome as pedidoVendedorNome,
case p.TipoPessoa
	 when 'Jurídica' then p.Inscricao 
	 else p.RG
	 end 
   	pedidoPessoaInscricao,
	pd.StatusEntregaVisivel,
	p.Nome as pedidoPessoaNome,
	pe.Endereco as pedidoPessoaEndereco,
	pe.Numero as pedidoPessoaEnderecoNumero,
	pe.Bairro as pedidoPessoaEnderecoBairro,
	pe.Complemento as pedidoPessoaEnderecoComplemento,
	cp.Nome + ' - ' + up.Sigla as pedidoPessoaEnderecoCidade,
	pe.CEP as pedidoPessoaEnderecoCEP,
	p.CNPJ_CPF as pedidoPessoaCNPJ_CPF,
	pte.Numero as pedidoPessoaTelefone,
	cd.Nome as pedidoCondicao,
	pr.Codigo as ProdutoCodigo,
	pr.Nome as ProdutoDescricao,
	un.Sigla as ProdutoUN,	
	pi.Quantidade as ProdutoQuantidade,	
	ei.QuantidadeEntrega as QtdEntregue,
	(pi.Quantidade - (isnull((select sum(eiant.QuantidadeEntrega) 
								from entregaitem eiant 
								inner join Entrega eant on eiant.IDEntrega = eant.id 
						where eant.idpedido = ent.idpedido and eant.id &lt;= ent.id and eiant.IDPedidoItem = ei.IDPedidoItem),0))) QtdRestante,
	convert(decimal(18,2),pi.Valor) as ProdutoValorUn,
	pi.Total as ProdutoValorTotal,
	pd.Observacao1 as PedidoObservacao,
	tp.Observacao as tipopedidoobservacao,
	pd.TotalProduto as PedidoTotalProduto,
	pd.ValorFrete as PedidoValorFrete,
	pd.TotalPedido as PedidoTotal,
	ent.Solicitante as SolicitanteEntrega,
	ent.Responsavel as ResponsavelEntrega,
	(select Sum(pitInt.Quantidade) from PedidoItem pitInt
		 join Pedido pdInt on pdInt.ID = pitInt.IDPedido
     where pdInt.ID = pd.ID
     group by pitInt.Quantidade) QtdPedido
from Entrega ent
	join EntregaItem ei on ei.IDEntrega = ent.ID
	join PedidoItem pi on ei.IDPedidoItem = pi.ID
	join Pedido pd on pi.IDPedido = pd.ID
	join Filial f on f.ID = pd.IDFilial
	join Cidade c on c.ID = f.IDCidade
	join UF u on u.ID = c.IDUF
	join Pessoa p on p.id = pd.IDPessoa
	join Condicao cd on cd.ID = pd.IDCondicao
	join PessoaEndereco as pe ON pe.ID = ISNULL((SELECT TOP (1) ID FROM PessoaEndereco WHERE(IDPessoa = p.ID) AND (bEntrega = 1)),(SELECT TOP (1) ID FROM PessoaEndereco WHERE(IDPessoa = p.ID) AND (bPrincipal = 1)))  
	join PessoaTelefone as pte ON pte.ID = (SELECT TOP (1) ID FROM PessoaTelefone WHERE(IDPessoa = p.ID) AND (bPrincipal = 1)) 
	join Cidade cp on cp.ID = pe.IDCidade
	join UF up on up.id = cp.IDUF
	join TipoPedido tp on tp.ID = pd.IDTipoPedido
	join Produto pr on pr.id = pi.IDProduto
	join Unidade un on un.ID = pr.IDUnidade
	join Pessoa pv on pv.ID = pd.IDVendedor
WHERE ent.Data between @Datainicial and DATEADD(DAY,1, @DataFinal)
and pd.ID in  (select * from F_RetornaTabelaLista(@IDPedido, ','))
</CommandText>
