            <MainSource>
              <DbSource ConnectionRef="cwkcwkGestaoConnectionString (Settings)" DbObjectName="SELECT pd.ID AS CodigoPedido, f.fantasia AS FilialNome, f.CNPJ AS FilialCNPJ, f.Inscricao AS FilialInscricao, f.Endereco + ' ' + f.Numero AS FilialEndereco, f.Bairro AS FilialBairro, c.Nome AS FilialCidade, u.Sigla AS FilialUF, f.CEP AS FilialCEP, f.Telefone AS FilialTelefone, f.Email AS FilialEmail, pi.ID IDPedidoItem, pi.ProdutoNome, pi.Unidade UnidadePedidoItem, ISNULL(SUM(EI.QuantidadeEntrega),0) QtdEntregue, pi.Quantidade - ISNULL(SUM(EI.QuantidadeEntrega),0) QtdRestante, CASE tp.tipo WHEN '1' THEN 'Entrega: Orçamento' ELSE 'Entrega: Pedido' END tipopedido, f.Email AS EmpresaEmail, pd.Numero AS Pedidonumero, CONVERT(varchar, pd.Dt, 103) AS DataEmissao, CONVERT(varchar, pd.Dt, 14) AS HoraEmissao, CONVERT(varchar,(pd.Dt + pd.Validade), 103) AS pedidoValidade, CASE p.TipoPessoa WHEN 'Jurídica' THEN p.Inscricao ELSE p.RG END pedidoPessoaInscricao, pd.StatusEntregaVisivel, p.Nome AS pedidoPessoaNome, pe.Endereco AS pedidoPessoaEndereco, pe.Numero AS pedidoPessoaEnderecoNumero, pe.Bairro AS pedidoPessoaEnderecoBairro, pe.Complemento AS pedidoPessoaEnderecoComplemento, cp.Nome + ' - ' + up.Sigla AS pedidoPessoaEnderecoCidade, pe.CEP AS pedidoPessoaEnderecoCEP, p.CNPJ_CPF AS pedidoPessoaCNPJ_CPF, pte.Numero AS pedidoPessoaTelefone, cd.Nome AS pedidoCondicao, pr.Codigo AS ProdutoCodigo, pr.Nome AS ProdutoDescricao, un.Sigla AS ProdutoUN, pi.Quantidade AS ProdutoQuantidade, CONVERT(decimal(18,2),pi.Valor) AS ProdutoValorUn, pi.Total AS ProdutoValorTotal, pd.Observacao1 AS PedidoObservacao, tp.Observacao AS tipopedidoobservacao, pd.TotalProduto AS PedidoTotalProduto, pd.ValorFrete AS PedidoValorFrete, pd.TotalPedido AS PedidoTotal, (SELECT TOP (1) eInt.Solicitante FROM Entrega eInt JOIN EntregaItem eiInt ON eInt.ID = eiInt.IDEntrega WHERE eiInt.IDPedidoItem = pi.ID) SolicitanteEntrega, (SELECT TOP (1) eInt.Responsavel FROM Entrega eInt JOIN EntregaItem eiInt ON eInt.ID = eiInt.IDEntrega WHERE eiInt.IDPedidoItem = pi.ID) ResponsavelEntrega FROM Pedido pd JOIN PedidoItem pi ON pi.IDPedido = pd.ID LEFT JOIN EntregaItem ei ON ei.IDPedidoItem = pi.ID JOIN Filial f ON f.ID = pd.IDFilial JOIN Cidade c ON c.ID = f.IDCidade JOIN UF u ON u.ID = c.IDUF JOIN Pessoa p ON p.id = pd.IDPessoa JOIN Condicao cd ON cd.ID = pd.IDCondicao JOIN PessoaEndereco AS pe ON pe.ID = ISNULL((SELECT TOP (1) ID FROM PessoaEndereco WHERE(IDPessoa = p.ID) AND (bEntrega = 1)),(SELECT TOP (1) ID FROM PessoaEndereco WHERE(IDPessoa = p.ID) AND (bPrincipal = 1))) JOIN PessoaTelefone AS pte ON pte.ID = (SELECT TOP (1) ID FROM PessoaTelefone WHERE(IDPessoa = p.ID) AND (bPrincipal = 1)) JOIN Cidade cp ON cp.ID = pe.IDCidade JOIN UF up ON up.id = cp.IDUF JOIN TipoPedido tp ON tp.ID = pd.IDTipoPedido JOIN Produto pr ON pr.id = pi.IDProduto JOIN Unidade un ON un.ID = pr.IDUnidade JOIN Pessoa pv ON pv.ID = pd.IDVendedor2 WHERE pd.Dt BETWEEN @Datainicial AND DATEADD(DAY,1, @DataFinal) AND pd.ID IN (SELECT * FROM F_RetornaTabelaLista(@IDPedido, ',')) GROUP BY pd.ID, pi.ID, pi.ProdutoNome, pi.Unidade, pi.Quantidade, f.fantasia, f.CNPJ, f.Inscricao, f.Endereco, f.Numero, f.Bairro, c.Nome, u.Sigla, f.CEP, f.Telefone, f.Email, p.TipoPessoa, pd.StatusEntregaVisivel, p.Nome, pe.Endereco, pe.Numero, pe.Bairro, pe.Complemento, cp.Nome, pe.CEP, p.CNPJ_CPF, pte.Numero, cd.Nome, pr.Codigo, pr.Nome, un.Sigla, pi.Total, pd.Observacao1, tp.Observacao, tp.tipo, pd.Numero, pd.Dt, pd.Validade, pd.TotalProduto, pd.ValorFrete, pd.TotalPedido, p.Inscricao, p.RG, up.Sigla, pi.Valor" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select r.* from (select 
					pd.ID as CodigoPedido,
					f.fantasia as FilialNome,
					f.CNPJ as FilialCNPJ,
					f.Inscricao as FilialInscricao,
					f.Endereco + ' ' + f.Numero as FilialEndereco,
					f.Bairro as FilialBairro,
					c.Nome as FilialCidade,
					u.Sigla as FilialUF,
					f.CEP as FilialCEP,
					f.Telefone as FilialTelefone,
					f.Email as FilialEmail,
					pi.ID IDPedidoItem, 
					pi.ProdutoNome, 
					pi.Unidade UnidadePedidoItem, 
					ISNULL(SUM(EI.QuantidadeEntrega),0) QtdEntregue,
					pi.Quantidade - ISNULL(SUM(EI.QuantidadeEntrega),0) QtdRestante,
					case tp.tipo 
					when '1' then 'Entrega: Orçamento'
					else 'Entrega: Pedido'
					end
					tipopedido,
					f.Email as EmpresaEmail,
					pd.Numero as Pedidonumero,
					CONVERT(varchar, pd.Dt, 103) as DataEmissao,
					CONVERT(varchar, pd.Dt, 14) as HoraEmissao,
					CONVERT(varchar,(pd.Dt + pd.Validade), 103) as pedidoValidade,
					case p.TipoPessoa
					 when 'Jurídica' then p.Inscricao 
					 else p.RG
					 end 
   					pedidoPessoaInscricao,
					pd.StatusEntregaVisivel,
					p.Nome as pedidoPessoaNome,
					pe.Endereco as pedidoPessoaEndereco,
					pe.Numero as pedidoPessoaEnderecoNumero,
					pe.Bairro as pedidoPessoaEnderecoBairro,
					pe.Complemento as pedidoPessoaEnderecoComplemento,
					cp.Nome + ' - ' + up.Sigla as pedidoPessoaEnderecoCidade,
					pe.CEP as pedidoPessoaEnderecoCEP,
					p.CNPJ_CPF as pedidoPessoaCNPJ_CPF,
					pte.Numero as pedidoPessoaTelefone,
					cd.Nome as pedidoCondicao,
					pr.Codigo as ProdutoCodigo,
					pr.Nome as ProdutoDescricao,
					un.Sigla as ProdutoUN,	
					pi.Quantidade as ProdutoQuantidade,	
					convert(decimal(18,2),pi.Valor) as ProdutoValorUn,
					pi.Total as ProdutoValorTotal,
					pd.Observacao1 as PedidoObservacao,
					tp.Observacao as tipopedidoobservacao,
					pd.TotalProduto as PedidoTotalProduto,
					pd.ValorFrete as PedidoValorFrete,
					pd.TotalPedido as PedidoTotal,
					(select TOP (1) eInt.Solicitante from Entrega eInt join EntregaItem eiInt on eInt.ID = eiInt.IDEntrega where eiInt.IDPedidoItem = pi.ID) SolicitanteEntrega,
					(select TOP (1) eInt.Responsavel from Entrega eInt join EntregaItem eiInt on eInt.ID = eiInt.IDEntrega where eiInt.IDPedidoItem = pi.ID) ResponsavelEntrega
				from Pedido pd
				join PedidoItem pi on pi.IDPedido = pd.ID 
				left join EntregaItem ei on ei.IDPedidoItem = pi.ID
				join Filial f on f.ID = pd.IDFilial
				join Cidade c on c.ID = f.IDCidade
				join UF u on u.ID = c.IDUF
				join Pessoa p on p.id = pd.IDPessoa
				join Condicao cd on cd.ID = pd.IDCondicao
				join PessoaEndereco as pe ON pe.ID = ISNULL((SELECT TOP (1) ID FROM PessoaEndereco WHERE(IDPessoa = p.ID) AND (bEntrega = 1)),(SELECT TOP (1) ID FROM PessoaEndereco WHERE(IDPessoa = p.ID) AND (bPrincipal = 1)))  
				join PessoaTelefone as pte ON pte.ID = (SELECT TOP (1) ID FROM PessoaTelefone WHERE(IDPessoa = p.ID) AND (bPrincipal = 1)) 
				join Cidade cp on cp.ID = pe.IDCidade
				join UF up on up.id = cp.IDUF
				join TipoPedido tp on tp.ID = pd.IDTipoPedido
				join Produto pr on pr.id = pi.IDProduto
				join Unidade un on un.ID = pr.IDUnidade
				join Pessoa pv on pv.ID = pd.IDVendedor
				where pd.Dt between @Datainicial and DATEADD(DAY,1, @DataFinal)
				and pd.ID in (select * from F_RetornaTabelaLista(@IDPedido, ','))
				group by pd.ID,
						 pi.ID,
						 pi.ProdutoNome,
						 pi.Unidade,
						 pi.Quantidade,
						 f.fantasia,
						 f.CNPJ,
						 f.Inscricao,
						 f.Endereco,
						 f.Numero,
						 f.Bairro,
						 c.Nome,
						 u.Sigla,
						 f.CEP,
						 f.Telefone,
						 f.Email,
						 p.TipoPessoa,
						 pd.StatusEntregaVisivel,
						 p.Nome,
						 pe.Endereco,
						 pe.Numero,
						 pe.Bairro,
						 pe.Complemento,
						 cp.Nome,
						 pe.CEP,
						 p.CNPJ_CPF,
						 pte.Numero,
						 cd.Nome,
						 pr.Codigo,
						 pr.Nome,
						 un.Sigla,
						 pi.Total,
						 pd.Observacao1,
						 tp.Observacao,
						 tp.tipo,
						 pd.Numero,
						 pd.Dt,
						 pd.Validade, 
						 pd.TotalProduto,
						 pd.ValorFrete,
						 pd.TotalPedido,
						 p.Inscricao,
						 p.RG,
						 up.Sigla,
						 pi.Valor) as r
where r.QtdRestante &gt; 0
order by r.IDPedidoItem
                                </CommandText>
