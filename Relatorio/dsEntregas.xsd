                      <Parameter AllowDbNull="true" AutogeneratedName="Datainicial" ColumnName="Data" DataSourceName="Gestao.dbo.Entrega" DataTypeServer="datetime" DbType="DateTime" Direction="Input" ParameterName="@Datainicial" Precision="0" ProviderType="DateTime" Scale="0" Size="8" SourceColumn="Data" SourceColumnNullMapping="false" SourceVersion="Current" />
