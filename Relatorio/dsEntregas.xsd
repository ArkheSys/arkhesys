          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="EntregaSelecionadaTableAdapter" GeneratorDataComponentClassName="EntregaSelecionadaTableAdapter" Name="EntregaSelecionada" UserDataComponentName="EntregaSelecionadaTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="cwkcwkGestaoConnectionString (Settings)" DbObjectName="SELECT f.ID AS IDFilial, p.ID AS IDPessoa, pd.ID AS IDPedido, ent.ID AS IDEntrega, pi.ID AS IDPedidoItem, pi.ProdutoNome, pi.Unidade AS ProdutoUnidade, ent.Solicitante, ent.Responsavel, pd.ID AS CodigoPedido, pd.StatusEntregaVisivel AS PedidoEntregaStatus, CASE pd.Status WHEN 1 THEN 'Aberto' WHEN 2 THEN 'Faturado' WHEN 3 THEN 'Cancelado' WHEN 4 THEN 'Faturado Parcial' WHEN 5 THEN 'Fechado' WHEN 6 THEN 'Aguardando Expedição' WHEN 7 THEN 'Expedição' WHEN 8 THEN 'Aguardando Faturamento' WHEN 9 THEN 'Aguardando Pagamento' WHEN 10 THEN 'Aguardando Aprovação' ELSE '' END StatusPedidoVisivel, CONVERT(varchar, ent.Data, 103) AS DataEntrega, CONVERT(varchar, ent.Data, 14) AS HoraEntrega, f.fantasia AS FilialNome, f.CNPJ AS FilialCNPJ, f.Inscricao AS FilialInscricao, f.Endereco + ' ' + f.Numero AS FilialEndereco, f.Bairro AS FilialBairro, c.Nome AS FilialCidade, u.Sigla AS FilialUF, f.CEP AS FilialCEP, f.Telefone AS FilialTelefone, f.Email AS FilialEmail, CASE tp.tipo WHEN '1' THEN 'Entrega: Orçamento' ELSE 'Entrega: Pedido' END tipopedido, f.Email AS EmpresaEmail, pd.Numero AS Pedidonumero, CONVERT(varchar, pd.Dt, 103) AS DataEmissao, CONVERT(varchar, pd.Dt, 14) AS HoraEmissao, CONVERT(varchar,(pd.Dt + pd.Validade), 103) AS pedidoValidade, pv.nome AS pedidoVendedorNome, CASE p.TipoPessoa WHEN 'Jurídica' THEN p.Inscricao ELSE p.RG END pedidoPessoaInscricao, pd.StatusEntregaVisivel, p.Nome AS pedidoPessoaNome, pe.Endereco AS pedidoPessoaEndereco, pe.Numero AS pedidoPessoaEnderecoNumero, pe.Bairro AS pedidoPessoaEnderecoBairro, pe.Complemento AS pedidoPessoaEnderecoComplemento, cp.Nome + ' - ' + up.Sigla AS pedidoPessoaEnderecoCidade, pe.CEP AS pedidoPessoaEnderecoCEP, p.CNPJ_CPF AS pedidoPessoaCNPJ_CPF, pte.Numero AS pedidoPessoaTelefone, cd.Nome AS pedidoCondicao, pr.Codigo AS ProdutoCodigo, pr.Nome AS ProdutoDescricao, un.Sigla AS ProdutoUN, pi.Quantidade AS ProdutoQuantidade, ISNULL(SUM(EI.QuantidadeEntrega),0) AS QtdEntregue, (pi.Quantidade - (isnull((SELECT SUM(eiant.QuantidadeEntrega) FROM entregaitem eiant INNER JOIN Entrega eant ON eiant.IDEntrega = eant.id WHERE eant.idpedido = ent.idpedido AND eant.id &lt;= ent.id AND eiant.IDPedidoItem = ei.IDPedidoItem),0))) QtdRestante, CONVERT(decimal(18,2),pi.Valor) AS ProdutoValorUn, pi.Total AS ProdutoValorTotal, pd.Observacao1 AS PedidoObservacao, tp.Observacao AS tipopedidoobservacao, pd.TotalProduto AS PedidoTotalProduto, pd.ValorFrete AS PedidoValorFrete, pd.TotalPedido AS PedidoTotal, ent.Solicitante AS SolicitanteEntrega, ent.Responsavel AS ResponsavelEntrega FROM Entrega ent JOIN EntregaItem ei ON ei.IDEntrega = ent.ID JOIN PedidoItem pi ON ei.IDPedidoItem = pi.ID JOIN Pedido pd ON pi.IDPedido = pd.ID JOIN Filial f ON f.ID = pd.IDFilial JOIN Cidade c ON c.ID = f.IDCidade JOIN UF u ON u.ID = c.IDUF JOIN Pessoa p ON p.id = pd.IDPessoa JOIN Condicao cd ON cd.ID = pd.IDCondicao JOIN PessoaEndereco AS pe ON pe.ID = ISNULL((SELECT TOP (1) ID FROM PessoaEndereco WHERE(IDPessoa = p.ID) AND (bEntrega = 1)),(SELECT TOP (1) ID FROM PessoaEndereco WHERE(IDPessoa = p.ID) AND (bPrincipal = 1))) JOIN PessoaTelefone AS pte ON pte.ID = (SELECT TOP (1) ID FROM PessoaTelefone WHERE(IDPessoa = p.ID) AND (bPrincipal = 1)) JOIN Cidade cp ON cp.ID = pe.IDCidade JOIN UF up ON up.id = cp.IDUF JOIN TipoPedido tp ON tp.ID = pd.IDTipoPedido JOIN Produto pr ON pr.id = pi.IDProduto JOIN Unidade un ON un.ID = pr.IDUnidade JOIN Pessoa pv ON pv.ID = pd.IDVendedor GROUP BY f.ID, p.ID, pd.ID, ent.ID, pi.ID, pi.ProdutoNome, pi.Unidade, pi.Quantidade, ent.Solicitante, ent.Responsavel, pd.ID, pd.StatusEntregaVisivel, pd.Status, f.fantasia, f.CNPJ, f.Inscricao, f.Endereco, f.Numero, f.Bairro, c.Nome, u.Sigla, f.CEP, f.Telefone, f.Email, ent.Data, tp.tipo, f.Email, pd.Numero, pv.apelido, pd.Dt, p.TipoPessoa, pd.StatusEntregaVisivel, p.Nome, pe.Endereco, pe.Numero, pe.Bairro, pe.Complemento, cp.Nome, up.Sigla, pe.CEP, p.CNPJ_CPF, pte.Numero, cd.Nome, pr.Codigo, pr.Nome, un.Sigla, pi.Quantidade, ei.QuantidadeEntrega, pi.Valor, pi.Total, pd.Observacao1, tp.Observacao, pd.TotalProduto, pd.ValorFrete, pd.TotalPedido, ent.Solicitante, ent.Responsavel, pd.Validade, p.Inscricao, p.RG, ent.IDPedido, ei.IDPedidoItem" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>--Entrega Selecionada
 
	select r.* from (select
					f.ID as IDFilial,
					p.ID as IDPessoa,
					pd.ID as IDPedido,
					ent.ID as IDEntrega,
					pi.ID as IDPedidoItem, 
					pi.ProdutoNome, 
					pi.Unidade as ProdutoUnidade, 
					ent.Solicitante,
					ent.Responsavel,
					pd.ID as CodigoPedido,
					pd.StatusEntregaVisivel as PedidoEntregaStatus,
					case pd.Status
					when 1 then 'Aberto'
					when 2 then 'Faturado'
					when 3 then 'Cancelado'
					when 4 then 'Faturado Parcial'
					when 5 then 'Fechado'
					when 6 then 'Aguardando Expedição'
					when 7 then 'Expedição'
					when 8 then 'Aguardando Faturamento'
					when 9 then 'Aguardando Pagamento'
					when 10 then 'Aguardando Aprovação'
					else ''
					end StatusPedidoVisivel,
					Convert(varchar, ent.Data, 103) as DataEntrega,
					Convert(varchar, ent.Data, 14) as HoraEntrega,
					f.fantasia as FilialNome,
					f.CNPJ as FilialCNPJ,
					f.Inscricao as FilialInscricao,
					f.Endereco + ' ' + f.Numero as FilialEndereco,
					f.Bairro as FilialBairro,
					c.Nome as FilialCidade,
					u.Sigla as FilialUF,
					f.CEP as FilialCEP,
					f.Telefone as FilialTelefone,
					f.Email as FilialEmail,
					case tp.tipo 
					when '1' then 'Entrega: Orçamento'
					else 'Entrega: Pedido'
					end
					tipopedido,
					f.Email as EmpresaEmail,
					pd.Numero as Pedidonumero,
					CONVERT(varchar, pd.Dt, 103) as DataEmissao,
					CONVERT(varchar, pd.Dt, 14) as HoraEmissao,
					CONVERT(varchar,(pd.Dt + pd.Validade), 103) as pedidoValidade,
					pv.apelido as pedidoVendedorNome,
				case p.TipoPessoa
					 when 'Jurídica' then p.Inscricao 
					 else p.RG
					 end 
   					pedidoPessoaInscricao,
					pd.StatusEntregaVisivel,
					p.Nome as pedidoPessoaNome,
					pe.Endereco as pedidoPessoaEndereco,
					pe.Numero as pedidoPessoaEnderecoNumero,
					pe.Bairro as pedidoPessoaEnderecoBairro,
					pe.Complemento as pedidoPessoaEnderecoComplemento,
					cp.Nome + ' - ' + up.Sigla as pedidoPessoaEnderecoCidade,
					pe.CEP as pedidoPessoaEnderecoCEP,
					p.CNPJ_CPF as pedidoPessoaCNPJ_CPF,
					pte.Numero as pedidoPessoaTelefone,
					cd.Nome as pedidoCondicao,
					pr.Codigo as ProdutoCodigo,
					pr.Nome as ProdutoDescricao,
					un.Sigla as ProdutoUN,	
					pi.Quantidade as ProdutoQuantidade,	
					ISNULL(SUM(EI.QuantidadeEntrega),0) as QtdEntregue,
					(pi.Quantidade - (isnull((select sum(eiant.QuantidadeEntrega) 
													from entregaitem eiant 
													inner join Entrega eant on eiant.IDEntrega = eant.id 
											where eant.idpedido = ent.idpedido and eant.id &lt;= ent.id and eiant.IDPedidoItem = ei.IDPedidoItem),0))) QtdRestante,
					convert(decimal(18,2),pi.Valor) as ProdutoValorUn,
					pi.Total as ProdutoValorTotal,
					pd.Observacao1 as PedidoObservacao,
					tp.Observacao as tipopedidoobservacao,
					pd.TotalProduto as PedidoTotalProduto,
					pd.ValorFrete as PedidoValorFrete,
					pd.TotalPedido as PedidoTotal,
					ent.Solicitante as SolicitanteEntrega,
					ent.Responsavel as ResponsavelEntrega
				from Entrega ent
					join EntregaItem ei on ei.IDEntrega = ent.ID
					join PedidoItem pi on ei.IDPedidoItem = pi.ID
					join Pedido pd on pi.IDPedido = pd.ID
					join Filial f on f.ID = pd.IDFilial
					join Cidade c on c.ID = f.IDCidade
					join UF u on u.ID = c.IDUF
					join Pessoa p on p.id = pd.IDPessoa
					join Condicao cd on cd.ID = pd.IDCondicao
					JOIN PessoaEndereco as pe ON pe.ID = ISNULL((SELECT TOP (1) ID FROM PessoaEndereco WHERE(IDPessoa = p.ID) AND (bEntrega = 1)),(SELECT TOP (1) ID FROM PessoaEndereco WHERE(IDPessoa = p.ID) AND (bPrincipal = 1)))  
					JOIN PessoaTelefone as pte ON pte.ID = (SELECT TOP (1) ID FROM PessoaTelefone WHERE(IDPessoa = p.ID) AND (bPrincipal = 1)) 
					join Cidade cp on cp.ID = pe.IDCidade
					join UF up on up.id = cp.IDUF
					join TipoPedido tp on tp.ID = pd.IDTipoPedido
					join Produto pr on pr.id = pi.IDProduto
					join Unidade un on un.ID = pr.IDUnidade
					join Pessoa pv on pv.ID = pd.IDVendedor
					group by f.ID, p.ID, pd.ID, ent.ID,	pi.ID, pi.ProdutoNome, pi.Unidade, pi.Quantidade, ent.Solicitante, ent.Responsavel,
						 pd.ID,	pd.StatusEntregaVisivel, pd.Status,	f.fantasia, f.CNPJ,	f.Inscricao, f.Endereco, f.Numero, f.Bairro, 
						 c.Nome, u.Sigla, f.CEP, f.Telefone, f.Email, ent.Data, tp.tipo, f.Email, pd.Numero, pv.apelido, pd.Dt,
						 p.TipoPessoa, pd.StatusEntregaVisivel,	p.Nome,	pe.Endereco, pe.Numero, pe.Bairro, pe.Complemento, cp.Nome, 
						 up.Sigla, pe.CEP, p.CNPJ_CPF, pte.Numero, cd.Nome, pr.Codigo, pr.Nome,	un.Sigla, pi.Quantidade, ei.QuantidadeEntrega,
						 pi.Valor, pi.Total,	pd.Observacao1,	tp.Observacao, pd.TotalProduto,	pd.ValorFrete,
						 pd.TotalPedido, ent.Solicitante, ent.Responsavel, pd.Validade, p.Inscricao, p.RG, ent.IDPedido, ei.IDPedidoItem) r
where IDEntrega = @IDEntrega
order by r.IDEntrega
</CommandText>
                    <Parameters>
