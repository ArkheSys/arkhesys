          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="DataTable1TableAdapter" GeneratorDataComponentClassName="DataTable1TableAdapter" Name="DataTable1" UserDataComponentName="DataTable1TableAdapter">
            <MainSource>
              <DbSource ConnectionRef="cwkcwkGestaoConnectionString (Settings)" DbObjectName="SELECT prod.Codigo AS 'ProdCodigo', prod.Nome AS 'ProdNome', UN.Sigla AS 'UNSigla', ge.Nome AS 'GrupoEstoqueNome', (SELECT dbo.f_SaldoEstoqueLocalEstoque(LE.ID, prod.ID)) AS 'EstoqueFisico', (SELECT dbo.f_SaldoPedidosEstoqueEnt_Sai(prod.ID, 2,@TipoOrcamento,@TipoRequisicao,@dataIni,@dataFim)) AS 'ReqSaida', (SELECT dbo.f_SaldoPedidosEstoqueEnt_Sai(prod.ID, 1,@TipoOrcamento,@TipoRequisicao,@dataIni,@dataFim)) AS 'ReqCompra' FROM Produto prod INNER JOIN Estoque Estq ON Estq.IDProduto = prod.ID INNER JOIN LocalEstoque LE ON LE.ID = Estq.IDLocalEstoque INNER JOIN Unidade UN ON UN.ID = prod.IDUnidade INNER JOIN GrupoEstoque ge ON ge.ID = prod.IDGrupoEstoque WHERE prod.IDGrupoEstoque IN (SELECT * FROM dbo.F_RetornaTabelaLista(@idGrupoEstoque,',')) AND LE.IDFilial IN (SELECT * FROM dbo.F_RetornaTabelaLista(@idFilial,',')) AND LE.ID = @idLocalEstoque" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>
SELECT t.*, 
	CASE when ((t.EstoqueFisico - t.ReqSaida) + t.ReqCompra) &gt; 0 then 0 else ((t.EstoqueFisico - t.ReqSaida) + t.ReqCompra) END as 'necessidade',
	((t.EstoqueFisico - t.ReqSaida) + t.ReqCompra) as 'saldo'
FROM(
SELECT prod.Codigo as 'ProdCodigo',
       prod.Nome as 'ProdNome',
       UN.Sigla as 'UNSigla',
       ge.Nome as 'GrupoEstoqueNome',
       
      (SELECT dbo.f_SaldoEstoqueLocalEstoque(LE.ID, prod.ID)) AS 'EstoqueFisico',
      (SELECT dbo.f_SaldoPedidosEstoqueEnt_Sai(prod.ID, 2,@TipoOrcamento,@TipoRequisicao,@dataIni,@dataFim)) AS 'ReqSaida',
      (SELECT dbo.f_SaldoPedidosEstoqueEnt_Sai(prod.ID, 1,@TipoOrcamento,@TipoRequisicao,@dataIni,@dataFim)) AS 'ReqCompra'
      
      FROM Produto prod
      INNER JOIN Estoque Estq on Estq.IDProduto = prod.ID
      INNER JOIN LocalEstoque LE on LE.ID = Estq.IDLocalEstoque
      INNER JOIN Unidade UN on UN.ID = prod.IDUnidade
      INNER JOIN GrupoEstoque ge on ge.ID = prod.IDGrupoEstoque
            
      WHERE prod.IDGrupoEstoque in (select * from dbo.F_RetornaTabelaLista(@idGrupoEstoque,','))
      AND LE.IDFilial in (select * from dbo.F_RetornaTabelaLista(@idFilial,','))
      AND LE.ID = @idLocalEstoque
      )t 
      </CommandText>
