                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        p.Codigo AS CodigoProduto, ni.Quantidade AS Qtd, ni.Dt AS DataEntrada, DATEDIFF(DAY, ni.Dt, GETDATE()) AS TempoEstoque, i.Ano, i.Sequencia
FROM            Produto AS p INNER JOIN
                         NotaItem AS ni ON p.ID = ni.IDProduto INNER JOIN
                         Nota AS n ON n.ID = ni.IDNota INNER JOIN
                         Identificador AS i ON i.ID = n.IDIdentificador
ORDER BY TempoEstoque DESC</CommandText>
