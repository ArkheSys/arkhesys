          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="Pedido06TableAdapter" GeneratorDataComponentClassName="Pedido06TableAdapter" Name="Pedido06TableAdapter" UserDataComponentName="Pedido06TableAdapter">
