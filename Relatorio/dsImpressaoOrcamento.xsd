              <DbSource ConnectionRef="cwkcwkGestaoConnectionString (Settings)" DbObjectName="" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>
						select
						f.fantasia as EmpresaNome,
						f.CNPJ as EmpresaCNPJ,
						f.Inscricao as EmpresaInscricao,
						f.Endereco + ' ' + f.Numero as EmpresaEndereco,
						f.Bairro as EmpresaBairro,
						c.Nome as EmpresaCidade,
						u.Sigla as EmpresaUF,
						f.CEP as EmpresaCEP,
						f.Telefone as EmpresaTelefone,
						case tp.tipo
						when '1' then 'Orçamento'
						else 'Pedido'
						end
						tipopedido,
						f.Email as EmpresaEmail,
						pd.Numero as Pedidonumero,
						CONVERT(varchar,pd.IncData, 103) + ' ' + SUBSTRING(CONVERT(Varchar,pd.incdata,108),0,6) as DataEmissao,
						CONVERT(varchar,(pd.Dt + pd.Validade), 103) as pedidoValidade,
						pv.nome as pedidoVendedorNome,
						case p.TipoPessoa
						when 'Jurídica' then p.Inscricao
						else p.RG
						end
						pedidoPessoaInscricao,
						p.Nome as pedidoPessoaNome,
						pe.Endereco as pedidoPessoaEndereco,
						pe.Numero as pedidoPessoaEnderecoNumero,
						pe.Bairro as pedidoPessoaEnderecoBairro,
						pe.Complemento as pedidoPessoaEnderecoComplemento,
						cp.Nome + ' - ' + up.Sigla as pedidoPessoaEnderecoCidade,
						pe.CEP as pedidoPessoaEnderecoCEP,
						p.CNPJ_CPF as pedidoPessoaCNPJ_CPF,
						pte.Numero as pedidoPessoaTelefone,
						cd.Nome as pedidoCondicao,
						pr.Codigo as ProdutoCodigo,
						pr.Nome as ProdutoDescricao,
						coalesce(pi.Observacao, '') as ObservacaoItem,
						un.Sigla as ProdutoUN,
						pi.Quantidade as ProdutoQuantidade,
						convert(decimal(18,2),pi.Valor) as ProdutoValorUn,
						pi.Total as ProdutoValorTotal,
						pd.Observacao1 as PedidoObservacao,
						tp.Observacao as tipopedidoobservacao,
						pd.TotalProduto as PedidoTotalProduto,
						pd.ValorFrete as PedidoValorFrete,
						pd.TotalPedido as PedidoTotal


						from Pedido pd
						left join Filial f on f.ID = pd.IDFilial
						left join Cidade c on c.ID = f.IDCidade
						left join UF u on u.ID = c.IDUF
						left join Pessoa p on p.id = pd.IDPessoa
						left join Condicao cd on cd.ID = pd.IDCondicao
						left join PedidoItem pi on pi.IDPedido = pd.ID
						LEFT JOIN PessoaEndereco as pe ON pe.ID = (SELECT TOP (1) ID FROM PessoaEndereco WHERE(IDPessoa = pd.IDpessoa) AND ((bEntrega = 1) or (bprincipal = 1)) order by bentrega desc, bprincipal desc)
						LEFT JOIN PessoaTelefone as pte ON pte.ID = (SELECT TOP (1) ID FROM PessoaTelefone WHERE(IDPessoa = p.ID) AND (bPrincipal = 1))
						left join cidade cp on cp.ID = pe.IDCidade
						left join UF up on up.id = cp.IDUF
						left join TipoPedido tp on tp.ID = pd.IDTipoPedido
						left join Produto pr on pr.id = pi.IDProduto
						left join Unidade un on un.ID = pr.IDUnidade
						left join Pessoa pv on pv.ID = pd.IDVendedor

						WHERE pd.ID = @pPedido


					</CommandText>
