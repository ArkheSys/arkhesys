                      <Parameter AllowDbNull="false" AutogeneratedName="pPedido" ColumnName="ID" DataSourceName="DEMOGESTAO.dbo.Pedido" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@pPedido" Precision="0" ProviderType="Int" Scale="0" Size="4" SourceColumn="ID" SourceColumnNullMapping="false" SourceVersion="Current" />
