          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="Pedido05TableAdapter" GeneratorDataComponentClassName="Pedido05TableAdapter" Name="Pedido05TableAdapter" UserDataComponentName="Pedido05TableAdapter">
            <MainSource>
              <DbSource ConnectionRef="cwkcwkGestaoConnectionString (Settings)" DbObjectName="SELECT f.nome AS EmpresaNome, f.CNPJ AS EmpresaCNPJ, f.Inscricao AS EmpresaInscricao, f.Endereco + ' ' + f.Numero AS EmpresaEndereco, f.Bairro AS EmpresaBairro, c.Nome AS EmpresaCidade, u.Sigla AS EmpresaUF, f.CEP AS EmpresaCEP, f.Telefone AS EmpresaTelefone, f.Email AS EmpresaEmail, pd.Numero AS Pedidonumero, CONVERT(varchar, pd.Dt, 103) AS DataEmissao, p.Nome AS pedidoPessoaNome, pe.Endereco AS pedidoPessoaEndereco, pe.Numero AS pedidoPessoaEnderecoNumero, pe.Bairro AS pedidoPessoaEnderecoBairro, pe.Complemento AS pedidoPessoaEnderecoComplemento, cp.Nome + ' - ' + up.Sigla AS pedidoPessoaEnderecoCidade, pe.CEP AS pedidoPessoaEnderecoCEP, p.CNPJ_CPF AS pedidoPessoaCNPJ_CPF, CASE p.TipoPessoa WHEN 'Jurídica' THEN p.Inscricao ELSE p.RG END pedidoPessoaInscricao, pte.Numero AS pedidoPessoaTelefone, pe.Contato AS pedidoPessoaContato, cd.Nome AS pedidoCondicao, pd.TotalPedido AS pedidoTotal, CONVERT(varchar,(pd.Dt + pd.Validade), 103) AS pedidoValidade, CASE tp.tipo WHEN '1' THEN 'Orçamento' ELSE 'Pedido' END tipopedido, pr.Codigo AS ProdutoCodigo, un.Sigla AS ProdutoUN, pi.Quantidade AS ProdutoQuantidade, pr.Nome AS ProdutoDescricao, pr.Localizacao AS ProdutoLocalizacao, pi.Valor AS ProdutoValorUn, pi.Total AS ProdutoValorTotal, pd.TotalProduto AS PedidoTotalProduto, pd.TotalST AS PedidoTotalST, pd.TotalIPI AS PedidoTotalIPI, (dbo.f_SaldoEstoqueLocalEstoque(tn.IDLocalEstoque, pi.idproduto) - dbo.f_PedSaldoEstoqueLocalEstoque(tn.IDLocalEstoque, pi.idproduto)) AS EstoqueEfetivo FROM Pedido pd LEFT JOIN Filial f ON f.ID = pd.IDFilial LEFT JOIN Cidade c ON c.ID = f.IDCidade LEFT JOIN UF u ON u.ID = c.IDUF LEFT JOIN Pessoa p ON p.id = pd.IDPessoa LEFT JOIN Condicao cd ON cd.ID = pd.IDCondicao LEFT JOIN PedidoItem pi ON pi.IDPedido = pd.ID LEFT JOIN PessoaEndereco AS pe ON pe.ID = (SELECT TOP (1) ID FROM PessoaEndereco WHERE(IDPessoa = p.ID) AND (bPrincipal = 1)) LEFT JOIN PessoaTelefone AS pte ON pte.ID = (SELECT TOP (1) ID FROM PessoaTelefone WHERE(IDPessoa = p.ID) AND (bPrincipal = 1)) LEFT JOIN cidade cp ON cp.ID = pe.IDCidade LEFT JOIN UF up ON up.id = cp.IDUF LEFT JOIN TipoPedido tp ON tp.ID = pd.IDTipoPedido LEFT JOIN Produto pr ON pr.id = pi.IDProduto LEFT JOIN Unidade un ON un.ID = pr.IDUnidade LEFT JOIN estoque es ON es.IDProduto = pr.ID AND es.IDLocalEstoque = tp.IDLocalEstoque LEFT JOIN TipoNota tn ON tn.ID = tp.IDTipoNota WHERE pd.ID = @pPedido" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select t.*,
			'AtendePedido' = Case when (t.ProdutoQuantidade &lt;= EstoqueEfetivo) then 'A'
	when (t.ProdutoQuantidade &gt; EstoqueEfetivo and EstoqueEfetivo &gt; 0) then 'P'	else 'N'
	end
from (
select

	f.nome as EmpresaNome,
	f.CNPJ as EmpresaCNPJ,
	f.Inscricao as EmpresaInscricao,
	f.Endereco + ' ' + f.Numero as EmpresaEndereco,
	f.Bairro as EmpresaBairro,
	c.Nome as EmpresaCidade,
	u.Sigla as EmpresaUF,
	f.CEP as EmpresaCEP,
	f.Telefone as EmpresaTelefone,
	f.Email as EmpresaEmail,   
	pd.Numero as Pedidonumero,
	CONVERT(varchar, pd.Dt, 103) as DataEmissao,
	p.Nome as pedidoPessoaNome,
	pe.Endereco as pedidoPessoaEndereco,
	pe.Numero as pedidoPessoaEnderecoNumero,
	pe.Bairro as pedidoPessoaEnderecoBairro,
	pe.Complemento as pedidoPessoaEnderecoComplemento,
	cp.Nome + ' - ' + up.Sigla as pedidoPessoaEnderecoCidade,
	pe.CEP as pedidoPessoaEnderecoCEP,
	p.CNPJ_CPF as pedidoPessoaCNPJ_CPF,
	case p.TipoPessoa
	 when 'Jurídica' then p.Inscricao 
	 else p.RG
	 end 
   	pedidoPessoaInscricao,
	pte.Numero as pedidoPessoaTelefone,
	pe.Contato as pedidoPessoaContato,
	cd.Nome as pedidoCondicao,
	pd.TotalPedido as pedidoTotal,
	CONVERT(varchar,(pd.Dt + pd.Validade), 103) as pedidoValidade,
	case tp.tipo 
	when '1' then 'Orçamento'
	else 'Pedido'
	end
	tipopedido,
	
	pr.Codigo as ProdutoCodigo,
	un.Sigla as ProdutoUN,
	pi.Quantidade as ProdutoQuantidade,
	pr.Nome as ProdutoDescricao,
	pr.Localizacao as ProdutoLocalizacao,
	pi.Valor as ProdutoValorUn,
	pi.Total as ProdutoValorTotal,
	pd.TotalProduto as PedidoTotalProduto,
	pd.TotalST as PedidoTotalST,
	pd.TotalIPI as PedidoTotalIPI,
	(dbo.f_SaldoEstoqueLocalEstoque(tn.IDLocalEstoque, pi.idproduto) - dbo.f_PedSaldoEstoqueLocalEstoque(tn.IDLocalEstoque, pi.idproduto)) as EstoqueEfetivo
	
	from Pedido pd
	left join Filial f on f.ID = pd.IDFilial
	left join Cidade c on c.ID = f.IDCidade
	left join UF u on u.ID = c.IDUF
	left join Pessoa p on p.id = pd.IDPessoa 
	left join Condicao cd on cd.ID = pd.IDCondicao
	left join PedidoItem pi on pi.IDPedido = pd.ID
	LEFT JOIN PessoaEndereco as pe ON pe.ID = (SELECT TOP (1) ID FROM PessoaEndereco WHERE(IDPessoa = p.ID) AND (bPrincipal = 1)) 
	LEFT JOIN PessoaTelefone as pte ON pte.ID = (SELECT TOP (1) ID FROM PessoaTelefone WHERE(IDPessoa = p.ID) AND (bPrincipal = 1)) 
	left join cidade cp on cp.ID = pe.IDCidade
	left join UF up on up.id = cp.IDUF
	left join TipoPedido tp on tp.ID = pd.IDTipoPedido
	left join Produto pr on pr.id = pi.IDProduto
	left join Unidade un on un.ID = pr.IDUnidade
	left join estoque es on es.IDProduto = pr.ID and es.IDLocalEstoque = tp.IDLocalEstoque
	left join TipoNota tn on tn.ID = tp.IDTipoNota

WHERE pd.ID = @pPedido
) t



</CommandText>
