          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="dtImpostosNFSeTableAdapter" GeneratorDataComponentClassName="dtImpostosNFSeTableAdapter" Name="dtImpostosNFSe" UserDataComponentName="dtImpostosNFSeTableAdapter">
