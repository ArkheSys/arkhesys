                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
	tg.Nome as 'TelGrupo',
	ttl.Descricao as 'TelDesc',
	sum(tf.total) as 'TelValor', 
	sum(convert(numeric(18,2),replace((tl.ValorComImposto),',','.'))) as 'TelSomaImpostos',
	sum(tl.TempoSegundos) as 'TelTempoSegundos'

from tel_fatura t
inner join Tel_Cliente tc on tc.Id = t.IdTel_Cliente
inner join Tel_GrupoCliente tg on tg.Id = tc.IdTel_GrupoCliente
inner join tel_faturaligacao tf on tf.IdTel_Fatura = t.ID
inner join Tel_Ligacao tl on tl.ID = tf.IdTel_Ligacao
INNER JOIN Tel_TipoLigacao ttl ON ttl.Id = tf.IdTel_TipoLigacao
where 
	tg.Id in (select * from F_RetornaTabelaLista(@in_GrupoCliente, ',')) and
	ttl.Id in (select * from F_RetornaTabelaLista(@in_TipoLigacao, ',')) and
	DATEPART (MONTH ,t.Referencia) = @RefMes and
	DATEPART (YEAR ,t.Referencia) = @RefAno
group by ttl.Descricao, tg.Nome</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="RefMes" ColumnName="" DataSourceName="" DataTypeServer="unknown" DbType="Decimal" Direction="Input" ParameterName="@RefMes" Precision="0" Scale="0" Size="0" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
