                      <Parameter AllowDbNull="false" AutogeneratedName="in_TipoLigacao" ColumnName="" DataSourceName="" DataTypeServer="varchar" DbType="AnsiString" Direction="Input" ParameterName="@in_TipoLigacao" Precision="0" Scale="0" Size="1024" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
