          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="dttaPedidoDimaggio" GeneratorDataComponentClassName="dttaPedidoDimaggio" Name="dtPedidoDimaggio" UserDataComponentName="dttaPedidoDimaggio">
            <MainSource>
              <DbSource ConnectionRef="cwkcwkGestaoConnectionString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
	  P.Codigo, P.Dt, P.DtValidade
	, P.PessoaNome, P.PessoaCNPJCPF, P.PessoaEndereco
	, P.PessoaBairro, P.PessoaCEP, P.PessoaCidade, P.PessoaUF
	, P.PessoaTelefone, P.TotalProduto, P.PercDesconto
	, P.ValorDesconto, P.ValorAcrescimo, P.TotalPedido
	, P.Observacao1, P.Observacao2, P.Observacao3
	, P.Numero, P.ComplementoNumero
	, ISNULL(P.Complemento,'') AS Complemento
	, P.DtPrazoEntrega, P.TotalServico
	, CASE WHEN P.bCaixasAcopladas = 1 THEN 'Sim' ELSE 'Não' END AS CaixasAcopladas
	, ISNULL(P.NumeroUnidades,0) AS NumeroUnidades
	, ISNULL(P.NumeroBanheirosPUnidade,0) AS NumeroBanheirosPUnidade
	, ISNULL(P.Garantia, '') AS Garantia
	, F.Telefone AS TelEmpresa, ISNULL(F.Site, '') AS SiteEmpresa

	, CASE WHEN F.Telefone IS NOT NULL THEN F.Telefone + ' ' ELSE '' END 
	+ CASE WHEN F.Email IS NOT NULL THEN '- E-MAIL: ' + F.Email + ' ' ELSE '' END 
	+ CASE WHEN F.Site IS NOT NULL THEN '- SITE: ' + F.Site ELSE '' END AS ContatoEmpresa
	, F.Endereco AS EndEmpresa, F.Email AS EmailEmpresa
	, F.Bairro AS BairroEmpresa, F.CEP AS CepEmpresa
	, C.Nome AS CidadeEmpresa, E.Sigla AS UFEmpresa

	, CASE WHEN F.Endereco IS NOT NULL THEN F.Endereco + ', ' ELSE '' END 
	+ CASE WHEN F.Bairro IS NOT NULL THEN F.Bairro + ', ' ELSE '' END 
	+ C.Nome + '-' + E.Sigla + '.' + ' CEP: ' + F.CEP AS EndCompEmpresa

	, COALESCE(V2.Nome, V1.Nome, '') AS Vendedor
	, COALESCE(V2.Email1, V2.Email2, (SELECT TOP(1) PEV1.Email FROM PessoaEmail PEV1)
	, V1.Email1, V1.Email2, (SELECT TOP(1) PEV2.Email FROM PessoaEmail PEV2), '') AS EmailVendedor

	, COALESCE(
		(SELECT TOP(1) PTV1.Numero FROM PessoaTelefone PTV1 
			WHERE PTV1.bPrincipal = 1 AND PTV1.Numero IS NOT NULL AND PTV1.Numero != '')
	,	(SELECT TOP(1) PTV2.Numero FROM PessoaTelefone PTV2 
			WHERE PTV2.bPrincipal = 1 AND PTV2.Numero IS NOT NULL AND PTV2.Numero != '')
	, '') AS TelVendedor

	, COALESCE(P.ReferenciasEmpresa, T.Referencias, '') AS RefsEmpresa

FROM Pedido P
LEFT JOIN FILIAL F ON P.IDFilial = F.ID
LEFT JOIN CIDADE C ON F.IDCidade = C.ID
LEFT JOIN UF E ON C.IDUF = E.ID
LEFT JOIN PESSOA V1 ON P.IDVendedor = V1.ID
LEFT JOIN PessoaEmail PEV1 ON V1.ID = PEV1.IDPessoa
LEFT JOIN PESSOA V2 ON P.IDVendedor2 = V2.ID
LEFT JOIN PessoaEmail PEV2 ON V2.ID = PEV2.IDPessoa
LEFT JOIN TipoPedido T ON P.IDTipoPedido = T.ID
LEFT JOIN Condicao CO ON P.IDCondicao = CO.ID

WHERE P.ID = @IDPedido</CommandText>
