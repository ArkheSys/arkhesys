                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select PIS.Sequencia, S.nome AS NomeServico, S.descricao AS DescricaoServico
      , ISNULL(PROD.Nome, '') AS Produto,PIS.CustoEFormasDePagamento
      , ISNULL(pit.Quantidade,0) AS Quantidade, S.InformacoesAdicionais, S.Imagem
from PedidoItemServico pis
inner join Servico s on pis.IDServico = s.id
left join PedidoItem pit on pit.IDPedido = pis.IDPedido 
	and (pit.IDProduto in (
		select IDProduto from ProdutoServico where IDServico = pis.IDServico)
	)
left join Produto prod on prod.id = pit.IDProduto

where pis.IDPedido = @IDPedido</CommandText>
