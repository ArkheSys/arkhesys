            <MainSource>
              <DbSource ConnectionRef="cwkcwkGestaoConnectionString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT  PedidoItem.Total AS ValorTotal,
        PedidoItem.Valor AS ValorUnitario, Pedido.ComplementoNumero as NumNota,
		Pedido.ValorAcrescimo as acrescimo, Pedido.ValorFrete as frete,
		pedido.ValorComissao as comissao, pedido.TotalIPI as IPI,
		Pedido.ValorDesconto as desconto,
		CASE Unidade.bQtdFracionada WHEN 0 THEN CAST(CONVERT(Int, PedidoItem.Quantidade) AS varchar) 
		ELSE CAST((PedidoItem.Quantidade) AS varchar) END AS Qtd, 
		PedidoItem.ProdutoNome AS Produto, Produto.Codigo AS CodigoProduto, 
		Pessoa.Nome AS cliente, Pedido.PessoaEndereco AS EnderecoCliente, 
		Pedido.PessoaBairro AS Bairro, Pedido.PessoaCNPJCPF AS CPFCNPJCliente, 
		Pedido.PessoaCidade AS CidadeCliente, Pedido.PessoaUF AS EstadoCliente, 
		Grupo1.Nome AS Marca, Grupo2.Nome AS Modelo, Pedido.PessoaCEP AS CepCliente, 
		Pedido.PessoaTelefone AS TelefoneCliente, Pedido.Validade AS ValidadePedido, 
		ISNULL ((SELECT TOP (1) Email FROM PessoaEmail WHERE (IDPessoa = Pessoa.ID)
		ORDER BY bVenda DESC), '') AS EmailCliente, Pessoa.Inscricao AS IECliente, Pedido.Complemento, 
		PessoaEndereco.Endereco + ', ' + PessoaEndereco.Numero + 
		(CASE WHEN PessoaEndereco.Complemento = '' THEN '' ELSE ', ' + 
		PessoaEndereco.Complemento END) AS EnderecoMontagem, 
		Transp.Nome AS Transportadora, Pedido.ValorFrete AS Frete, Cidade.Nome AS CidadeMontagem, UF.Sigla AS EstadoMontagem, 
        PedidoItem.InformacoesAdicionais AS ObservacaoPedido, RTRIM(ISNULL(Pedido.Observacao2, '')) AS Observacao2, 
        RTRIM(ISNULL(Pedido.Observacao3, '')) AS Observacao3, ISNULL ((SELECT TOP (1) Numero FROM PessoaTelefone 
		WHERE (IDPessoa = Pessoa.ID and bPrincipal = 1)	ORDER BY PessoaTelefone.ID DESC), '') AS TelefoneMontagem, 
        Unidade.bQtdFracionada AS QtdFracionada, PessoaEndereco.Contato, PedidoItem.Unidade, 
        Pedido.TotalST, Pedido.TotalProduto, Pedido.Numero, Pedido.DtDigitacao AS Data, 
        Pessoa.Fantasia + ' (' + CAST(Pessoa.Codigo AS varchar) + ')' AS Fantasia, 
        P2.Apelido AS Vendedor, Pedido.TotalPedido, Pedido.Codigo AS CodigoPedido, 
        Pedido.IncData AS IncDataPedido, 
        CASE Unidade.bQtdFracionada WHEN 0 THEN CONVERT(Int, PedidoItem.Quantidade) ELSE CAST(PedidoItem.Quantidade AS decimal(16, 2)) 
        END AS QtdDuasCasas
                         
FROM            Pedido LEFT JOIN
                         Pessoa as Transp ON Transp.ID  = Pedido.IDPessoaTransportadora INNER JOIN
                         TipoPedido ON TipoPedido.ID = Pedido.IDTipoPedido INNER JOIN
                         PedidoItem ON PedidoItem.IDPedido = Pedido.ID INNER JOIN
                         Produto ON Produto.ID = PedidoItem.IDProduto INNER JOIN
                         Unidade ON Unidade.ID = Produto.IDUnidade INNER JOIN
                         Pessoa ON Pessoa.ID = Pedido.IDPessoa LEFT OUTER JOIN
                         Grupo1 ON Grupo1.Id = Produto.IDGrupo1 LEFT OUTER JOIN
                         Grupo2 ON Grupo2.Id = Produto.IDGrupo2 LEFT OUTER JOIN
                         PessoaEndereco ON PessoaEndereco.ID =
                             (SELECT        TOP (1) ID
                               FROM            PessoaEndereco
                               WHERE        (IDPessoa = Pessoa.ID) AND (bEntrega = 1)) LEFT OUTER JOIN
                         Pessoa AS P2 ON Pedido.IDVendedor2 = P2.ID LEFT OUTER JOIN
                         Cidade ON Cidade.ID = PessoaEndereco.IDCidade LEFT OUTER JOIN
                         UF ON UF.ID = Cidade.IDUF

WHERE PEDIDO.ID = @pPedidoID</CommandText>
