                      <Parameter AllowDbNull="false" AutogeneratedName="pPedidoID" ColumnName="ID" DataSourceName="Gestao.dbo.Pedido" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@pPedidoID" Precision="0" ProviderType="Int" Scale="0" Size="4" SourceColumn="ID" SourceColumnNullMapping="false" SourceVersion="Current" />
