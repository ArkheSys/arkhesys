<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="FaturamentoAtendimento" table="FaturamentoAtendimento">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<property name="IncData" column="IncData" type="DateTime" />
	<property name="AltData" column="AltData" type="DateTime" />
	<property name="IncUsuario" column="IncUsuario" type="string" />
	<property name="AltUsuario" column="AltUsuario" type="string" />
	<many-to-one name="Cliente" column="IDPessoa" class="Pessoa" lazy="false"/>
	<many-to-one name="Servico" column="IDServico" class="Servico" lazy="false"/>
	<property name="Quantidade" column="Quantidade" type="Int32" />
	<property name="HorasAtendimento" column="HorasAtendimento" type="TimeSpan"/>
	<property name="HorasFaturamento" column="HorasFaturamento" type="TimeSpan"/>
	<property name="Valor" column="Valor" type="decimal" />
	<property name="Desconto" column="Desconto" type="decimal" />
	<property name="DespesasAdicionais" column="DespesasAdicionais" type="decimal" />
	<property name="Total" column="Total" type="decimal" />
  <property name="Observacao" column="Observacao" type="string" length="200" />

  <many-to-one name="OrdemServico" column="IDOSOrdemServico" class="OSOrdemServico" lazy="false"/>
   
  <bag name="AtendimentosVinculados" table="Atendimento" cascade="save-update" lazy="false" >
    <key column="IDFaturamentoAtendimento" />
    <one-to-many class="Atendimento" />
  </bag>

  </class>
</hibernate-mapping>
