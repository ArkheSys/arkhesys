<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="PLP" table="PLP">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<property name="Data" column="Data" type="DateTime" />
	<property name="Usuario" column="Usuario" type="string" />
	<property name="IDPLPWS" column="IDPLPWS" type="int" />
	<property name="Status" column="Status" type="bool" />
  <property name="StatusLegivel" column="StatusLegivel" type="string" />

    <bag name="PLPDetalhes" table="PLPDetalhe" inverse="true" cascade="all" lazy="false" >
      <key column="IDPLP" />
      <one-to-many class="PLPDetalhe" />
    </bag>
    
  </class>
</hibernate-mapping>
