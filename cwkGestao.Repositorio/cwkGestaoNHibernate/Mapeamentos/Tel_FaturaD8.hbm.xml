<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="Tel_FaturaD8" table="Tel_FaturaD8">
		<id name="ID" column="ID">
			<generator class="identity" />
		</id>

		<property name="Codigo" column="Codigo" type="Int32" />
		<many-to-one name="Filial" column="IdFilial" class="Filial" lazy="false" />
		<property name="DataCriacao" column="DataCriacao" type="DateTime" />
		<property name="DataVencimento" column="DataVencimento" type="DateTime" />
		<property name="NumeroDocumento" column="NumeroDocumento" type="string" length="50" />
		<property name="EspecieDocumento" column="EspecieDocumento" type="string" length="10" />
		<property name="Valor" column="Valor" type="Decimal" />
		<property name="Observacao" column="Observacao" type="string" length="1000" />
		<property name="LinhaDigitavel" column="LinhaDigitavel" type="string" />
		<property name="LinhaDigitavelComDv" column="LinhaDigitavelComDv" type="string" />		
		<property name="PixCopiaECola" column="PixCopiaECola" type="string" />
		<property name="Location" column="Location" type="string" />
		<property name="JsonPix" column="JsonPix" type="string" />

		<many-to-one name="Cliente" column="IdTel_Cliente" class="Tel_Cliente" lazy="false"  />
		<many-to-one name="Servico" column="IdTel_Servico" class="Tel_Servico" lazy="false" />
	</class>
</hibernate-mapping>