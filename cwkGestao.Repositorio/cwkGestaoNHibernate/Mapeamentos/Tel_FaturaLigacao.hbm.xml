<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="Tel_FaturaLigacao" table="Tel_FaturaLigacao">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>

    <property name="Sequencia" column="Sequencia" type="Int32" />
    <property name="Quantidade" column="Quantidade" type="Int32" />    
    <property name="ValorMinuto" column="ValorMinuto" type="Decimal" />
    <property name="Total" column="Total" type="Decimal" />
    <many-to-one name="Fatura" column="IdTel_Fatura" class="Tel_Fatura" />
    <many-to-one name="Ligacao" column="IdTel_Ligacao" class="Tel_Ligacao" />
    <many-to-one name="TipoLigacao" column="IdTel_TipoLigacao" class="Tel_TipoLigacao" />
    <property name="IncData" column="IncData" type="DateTime" />
    <property name="IncUsuario" column="IncUsuario" type="string" length="20" />
    <property name="AltData" column="AltData" type="DateTime" />
    <property name="AltUsuario" column="AltUsuario" type="string" length="20" />
  </class>
</hibernate-mapping>
