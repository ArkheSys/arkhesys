<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="Estoque" table="Estoque">

	<id name="ID" column="ID">
		<generator class="identity" /> 
	</id>
	<many-to-one name="Produto" column="IDProduto" class="Produto"/>
	<property name="Qtd_Estoque_Fisico" column="Qtd_Estoque_Fisico" type="decimal" />
	<property name="Qtd_Estoque_Pedido" column="Qtd_Estoque_Pedido" type="decimal" />
	<property name="Qtd_Compra_Pedido" column="Qtd_Compra_Pedido" type="decimal" />
	<property name="Localidade" column="Localidade" type="string" length="50" />
	<property name="Qtd_Estoque_Min" column="Qtd_Estoque_Min" type="decimal" />
  <property name="CustoMedioUnit" column="Custo_Medio_Unit" type="decimal" />
  <many-to-one name="LocalEstoque" column="IDLocalEstoque" class="LocalEstoque" lazy="false" />
  <bag name="Identificadores" table="EstoqueIdentificador"  inverse="true" cascade="all-delete-orphan" >
    <key column="IDEstoque" />
    <one-to-many class="EstoqueIdentificador" />
  </bag>
</class>
</hibernate-mapping>
		