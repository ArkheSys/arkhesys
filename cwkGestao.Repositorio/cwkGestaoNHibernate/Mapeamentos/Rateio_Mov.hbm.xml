<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="Rateio_Mov" table="Rateio_Mov">

    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <many-to-one name="Movimento" column="IDMovimento" class="Movimento" lazy="false"/>
    <many-to-one name="Projeto" column="IDProjeto" class="Projeto" lazy="false"/>
    <property name="Valor" column="Valor" type="decimal" />
    <property name="Sequencia" column="Sequencia" type="Int32" />
  </class>
</hibernate-mapping>
