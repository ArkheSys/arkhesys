<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="Condutor" table="Condutor">
		<id name="ID" column="ID">
			<generator class="identity" />
		</id>

		<property name="Cpf" column="Cpf" type="string" />
		<property name="Nome" column="Nome" type="string" />
		<property name="Ativo" column="Ativo" type="Int32" />
		<many-to-one name="Uf" column="IDUf" class="UF" lazy="false" />
	</class>
</hibernate-mapping>
