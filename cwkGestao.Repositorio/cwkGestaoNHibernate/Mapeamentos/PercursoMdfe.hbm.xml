<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="PercursoMdfe" table="PercursoMdfe">
		<id name="ID" column="ID">
			<generator class="identity" />
		</id>

		<property name="InicioViagem" column="InicioViagem" type="DateTime" />
		<many-to-one name="Mdfe" column="IDMdfe" class="Mdfe" lazy="false" />
		<many-to-one name="UFPercurso" column="IDUnidadeFederativaPercurso" class="UF" lazy="false" />
	</class>
</hibernate-mapping>
