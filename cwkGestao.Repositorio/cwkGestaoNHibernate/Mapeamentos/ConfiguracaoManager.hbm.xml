<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="ConfiguracaoManager" table="ConfiguracaoManager">

    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <property name="grupo" column="grupo" type="string" />
    <property name="usuario" column="usuario" type="string"  />
    <property name="senha" column="senha" type="string" />
    <property name="host" column="host" type="string" />
    <property name="CaminhoXMLNFSE" column="CaminhoXMLNFSE" type="string" />
    <property name="CaminhoPDFNFSE" column="CaminhoPDFNFSE" type="string" />

  </class>
</hibernate-mapping>
