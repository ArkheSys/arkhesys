<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="cepbr_endereco" table="cepbr_endereco">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<property name="cep" column="cep" type="string" />
	<property name="logradouro" column="logradouro" type="string" />
	<property name="tipo_logradouro" column="tipo_logradouro" type="string" />
	<property name="complemento" column="complemento" type="string" />
	<property name="local_end" column="local_end" type="string" />
	<many-to-one name="cep_cidade" column="id_cidade" class="cepbr_cidade" lazy="false"/>
	<many-to-one name="cep_bairro" column="id_bairro" class="cepbr_bairro" lazy="false"/>

  </class>
</hibernate-mapping>
