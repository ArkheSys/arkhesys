<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="PeriodoApuracaoContabil" table="PeriodoApuracaoContabil">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<many-to-one name="Filial" column="IDFilial" class="Filial" lazy="false" />
	<property name="Inicio" column="Inicio" type="Date" />
	<property name="Fim" column="Fim" type="Date" />
	<many-to-one name="Pessoa" column="IDPessoa" class="Pessoa" />

  </class>
</hibernate-mapping>
