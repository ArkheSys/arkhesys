<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="ProdutoEcommerceSimplificado" table="ProdutoEcommerceSimplificado">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>

	  <property name="IdEcommerce" column="IdEcommerce" type="Int32" />
	  <property name="Codigo" column="Codigo" type="Int32" />
    <property name="CodigoBarra" column="CodigoBarra" type="string" length="20" />
    <property name="Descricao" column="Descricao" type="string" length="200" />
    <property name="PrecoNormal" column="PrecoNormal" type="decimal" />
    <property name="PrecoPromocional" column="PrecoPromocional" type="decimal" />
    <property name="ImagemProduto" column="ImagemProduto" type="string" />
    <property name="IntegrarEcommerce" column="IntegrarEcommerce" type="Int32" />
    <property name="DataPromocionalInicial" column="DataPromocionalInicial" type="DateTime" />
    <property name="DataPromocionalFinal" column="DataPromocionalFinal" type="DateTime" />
    <property name="Observacao" column="Observacao" type="string" />
  </class>
</hibernate-mapping>