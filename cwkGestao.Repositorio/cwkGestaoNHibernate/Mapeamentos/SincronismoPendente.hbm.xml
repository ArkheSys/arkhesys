<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="SincronismoPendente" table="SincronismoPendente">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<property name="NomeCadastro" column="NomeCadastro" type="string" />
	<property name="IDCadastro" column="IDCadastro" type="Int32" />
  <property name="IncData" column="IncData" type="DateTime" />
  <property name="IncUsuario" column="IncUsuario" type="string" length="20" />
  <property name="Descricao" column="Descricao" type="string" length="500" />
  <property name="Erro" column="Erro" type="string" />
  <property name="IDIntegracao" column="IDIntegracao" type="Int32" />
  <property name="IDPedidoMagento" column="IDPedidoMagento" type="Int32" />
  <property name="ClienteNome" column="ClienteNome" type="string" />
  </class>
</hibernate-mapping>
