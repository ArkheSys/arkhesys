<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="CQEquipamento" table="CQEquipamento">

	<id name="ID" column="ID">
		<generator class="identity" /> 
	</id>
	<property name="Codigo" column="Codigo" type="Int32" />
	<many-to-one name="ControleQualidade" column="IDControleQualidade" class="ControleQualidade" />
					<many-to-one name="Equipamento" column="IDEquipamento" class="OSEquipamento" />
					<property name="QtdDefeito" column="QtdDefeito" type="decimal" />
</class>
</hibernate-mapping>
		