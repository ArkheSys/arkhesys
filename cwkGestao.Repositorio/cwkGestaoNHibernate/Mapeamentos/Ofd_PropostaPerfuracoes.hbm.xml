<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="Ofd_PropostaPerfuracoes" table="Ofd_PropostaPerfuracoes">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
  <many-to-one name="PropostaRevisao" column="IDOfd_PropostaRevisaoPerfuracoes" class="Ofd_PropostaRevisaoPerfuracoes" />
  <property name="Sequencia" column="Sequencia" type="Int32" />
  <property name="Estaca" column="Estaca" type="string" />
	<property name="Diametro" column="Diametro" type="Int32" />
	<property name="Quantidade" column="Quantidade" type="decimal" />
  <property name="Profundidade" column="Profundidade" type="decimal" />
	<property name="ValorUnitario" column="ValorUnitario" type="decimal" />
  <property name="IncData" column="IncData" type="DateTime" />
  <property name="IncUsuario" column="IncUsuario" type="string" length="20" />
  <property name="AltData" column="AltData" type="DateTime" />
  <property name="AltUsuario" column="AltUsuario" type="string" length="20" />

  </class>
</hibernate-mapping>
