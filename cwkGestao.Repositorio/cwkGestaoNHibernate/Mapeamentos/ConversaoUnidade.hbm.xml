<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="ConversaoUnidade" table="ConversaoUnidade">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<property name="Codigo" column="Codigo" type="string" length="4"/>
	<property name="Descricao" column="Descricao" type="string" length="100"/>
	<many-to-one name="UnidadeEnt" column="IDUnidadeEnt" class="Unidade" lazy="false"/>
	<many-to-one name="UnidadeSai" column="IDUnidadeSai" class="Unidade" lazy="false"/>
	<property name="FatorConversao" column="FatorConversao" type="decimal"  scale="5" precision="18"/>
  <property name="Padrao" column="Padrao" type="bool" />

  </class>
</hibernate-mapping>
