<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="GrupoEstoque" table="GrupoEstoque">

    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <property name="Codigo" column="Codigo" type="Int32" />
    <property name="Nome" column="Nome" type="string" length="60" />
    <property name="NomeReduzido" column="NomeReduzido" type="string" length="20" />
    <property name="Classificacao" column="Classificacao" type="string" length="30" />
    <property name="bShlGrupoEstoqueCampanha" column="bShlGrupoEstoqueCampanha" type="bool"/>
    <property name="CodShl" column="CodShl" type="Int32"/>
    <many-to-one name="GrupoEstoquePai" column="IDGrupoEstoque" class="GrupoEstoque" />
    <property name="BTitulo" column="bTitulo" type="bool" />
    <property name="ClassificacaoProduto" />
    <property name="CodClassProduto" column="CodClassProduto" type="string" length="5" />
    
    <bag name="Produtos" table="Produto">
      <key column="IDGrupoEstoque" />
      <one-to-many class="Produto" />
    </bag>
    <property name="IDIntegracao" column="IDIntegracao" type="Int32" />
  </class>
</hibernate-mapping>
		