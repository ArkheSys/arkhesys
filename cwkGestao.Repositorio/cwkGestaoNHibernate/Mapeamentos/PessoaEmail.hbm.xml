<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="PessoaEmail" table="PessoaEmail">

	<id name="ID" column="ID">
		<generator class="identity" /> 
	</id>
	<property name="Email" column="Email" type="string" length="100" />
	<property name="BFinanceiro" column="bFinanceiro" type="bool" />
	<property name="BVenda" column="bVenda" type="bool" />
	<property name="BCompra" column="bCompra" type="bool" />
	<property name="BAdministrativo" column="bAdministrativo" type="bool" />
	<property name="BContato" column="bContato" type="bool" />
	<property name="BSugestao" column="bSugestao" type="bool" />
	<property name="BMsn" column="bMsn" type="bool" />
  <property name="BNFe" column="bNFe" type="bool" />
  <property name="BNFSe" column="bNFSe" type="bool" />
    <many-to-one name="Pessoa" column="IDPessoa" class="Pessoa" />
				</class>
</hibernate-mapping>
		