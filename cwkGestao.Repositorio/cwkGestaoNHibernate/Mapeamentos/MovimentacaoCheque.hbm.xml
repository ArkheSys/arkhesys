<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="MovimentacaoCheque" table="MovimentacaoCheque">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>

    <property name="Codigo" column="Codigo" type="Int32" />
    <property name="NumeroCheque" column="NumeroCheque" type="string" length="20" />
    <property name="Valor" column="Valor" type="decimal" />
    <property name="DataMovimentacao" column="DataMovimentacao" type="DateTime" />
    <property name="DataCheque" column="DataCheque" type="DateTime" />
    <property name="ComplementoHist" column="ComplementoHist" type="string" length="200" />
    <many-to-one name="Pessoa" column="IDPessoa" class="Pessoa"/>
    <many-to-one name="Banco" column="IDBanco" class="Banco"/>
    <many-to-one name="BancoDestino" column="IDBancoDestino" class="Banco"/>
    <many-to-one name="Filial" column="IDFilial" class="Filial"/>
    <many-to-one name="Historico" column="IDHistorico" class="Historico"/>
    <many-to-one name="PlanoConta" column="IDPlanoConta" class="PlanoConta"/>
    <property name="IncData" column="IncData" type="DateTime" />
    <property name="IncUsuario" column="IncUsuario" type="string" length="20" />
    <property name="AltData" column="AltData" type="DateTime" />
    <property name="Dt" column="Dt" type="DateTime" />
    <property name="DtDigitacao" />
    <property name="Movimentacao"/>
    
  </class>
</hibernate-mapping>
