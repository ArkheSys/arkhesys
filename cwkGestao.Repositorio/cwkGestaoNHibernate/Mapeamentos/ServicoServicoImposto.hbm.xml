<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="ServicoServicoImposto" table="ServicoServicoImposto">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <many-to-one name="Servico" column="IDServico" class="Servico" lazy="false" />
    <many-to-one name="ServicoImposto" column="IDServicoImposto" class="ServicoImposto" lazy="false" />
    <property name="BaseRetencaoImposto" column="BaseRetencaoImposto" type="bool" /> 
  </class>
</hibernate-mapping>