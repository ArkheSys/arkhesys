<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="FormatoPacoteTransporte" table="FormatoPacoteTransporte">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<property name="Codigo" column="Codigo" type="Int32" />
	<property name="Descricao" column="Descricao" type="string" length="255"/>
  <property name="ComprimentoMinimo" column="ComprimentoMinimo" type="Int32" />
  <property name="ComprimentoMaximo" column="ComprimentoMaximo" type="Int32" />
  <property name="LarguraMinima" column="LarguraMinima" type="Int32" />
  <property name="LarguraMaxima" column="LarguraMaxima" type="Int32" />
  <property name="AlturaMinima" column="AlturaMinima" type="Int32" />
  <property name="AlturaMaxima" column="AlturaMaxima" type="Int32" />
  <property name="SomaMaximaDimensoes" column="SomaMaximaDimensoes" type="Int32" />
  <property name="DiametroMinimo" column="DiametroMinimo" type="Int32" />
  <property name="DiametroMaximo" column="DiametroMaximo" type="Int32" />
    
    <bag name="CorreiosPrecosPrazos" table="CorreioPrecoPrazo" lazy="false">
      <key column="IDFormatoPacoteTransporte" />
      <one-to-many class="CorreiosPrecoPrazo" />
    </bag>
  </class>
</hibernate-mapping>
