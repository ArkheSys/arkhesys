<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="OSAcompanhamento" table="OSAcompanhamento">

    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <many-to-one name="OSOrdemServico" column="IDOSOrdemServico" class="OSOrdemServico" lazy="false" />
    <property name="DataHora" column="DataHora" type="DateTime" />
    <property name="Ocorrencia" column="Ocorrencia" type="string" length="500" />
    <property name="Usuario" column="Usuario" type="string" length="20" />
    <property name="EhAutomatica" column="EhAutomatica" type="bool" />
  </class>
</hibernate-mapping>
