<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="PessoaTipoServicoTransporte" table="PessoaTipoServicoTransporte">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<many-to-one name="Pessoa" column="IDPessoa" class="Pessoa" lazy="false" />
	<many-to-one name="TipoServicoTransporte" column="IDTipoServicoTransporte" class="TipoServicoTransporte" lazy="false"/>
	<property name="CodShl" column="CodShl" type="Int32" />
	<property name="CodContrato" column="CodContrato" type="string" />

  </class>
</hibernate-mapping>
