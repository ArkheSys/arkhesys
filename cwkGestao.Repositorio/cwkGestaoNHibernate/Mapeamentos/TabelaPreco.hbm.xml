<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="TabelaPreco" table="TabelaPreco">

		<id name="ID" column="ID">
			<generator class="identity" />
		</id>
		<property name="Codigo" column="Codigo" type="Int32" />
		<property name="Nome" column="Nome" type="string" length="60" />
		<property name="BAtiva" column="bAtiva" type="bool" />
		<property name="SugestaoMargemLucro" column="SugestaoMargemLucro" type="decimal" />
		<property name="SugestaoPDesconto" column="SugestaoPDesconto" type="decimal" />
		<property name="SugestaoPAcrescimo" column="SugestaoPAcrescimo" type="decimal" />
		<property name="Promocional" column="Promocional" type="bool" />

	</class>
</hibernate-mapping>
