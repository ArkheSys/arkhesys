<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="Vinculo" table="Vinculo">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<many-to-one name="Pessoa" column="IDPessoa" class="Pessoa"  lazy="false"/>
	<property name="CodProduto" column="CodProduto" type="string" />
	<property name="CFOP" column="CFOP" type="string" />
	<many-to-one name="Produto" column="IDProduto" class="Produto" lazy="false" />
	<many-to-one name="IDCFOP" column="IDCFOP" class="TabelaCFOP"  lazy="false" />
  </class>
</hibernate-mapping>
