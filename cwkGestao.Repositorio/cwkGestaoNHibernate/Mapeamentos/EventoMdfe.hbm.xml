<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="EventoMdfe" table="EventoMdfe">
		<id name="ID" column="ID">
			<generator class="identity" />
		</id>

		<many-to-one name="MovimentoFiscalMdfe" column="IDMovimentoFiscalMdfe" class="MovimentoFiscalMdfe" lazy="false" />
		
		<property name="NseqEvento" column="NseqEvento" type="Int32" />
		<property name="DhEvento" column="DhEvento" type="DateTime" />
		<property name="DescEvento" column="DescEvento" type="string" />
		<property name="NProt" column="NProt" type="string" />
		<property name="XMotivo" column="XMotivo" type="string" />
		<property name="Cstat" column="Cstat" type="Int32" />
		<property name="TipoEvento" column="TipoEvento" type="Int32" />
	</class>
</hibernate-mapping>
