<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="MagentoConfiguracao" table="MagentoConfiguracao">

    <id name="ID" column="ID">
      <generator class="identity" />
    </id>    
    <property name="Usuario" column="Usuario" type="string" />
    <property name="Senha" column="Senha" type="string" />
    <property name="Uri" column="Uri" type="string" />
    <many-to-one name="Unidade" column="IDUnidade" class="Unidade" lazy="false"/>
    <many-to-one name="Fornecedor" column="IDFornecedor" class="Pessoa" lazy="false"/>
    <many-to-one name="GrupoEstoque" column="IDGrupoEstoque" class="GrupoEstoque" lazy="false"/>
    <many-to-one name="TabelaPreco" column="IDTabelaPreco" class="TabelaPreco" lazy="false"/>
    <many-to-one name="Condicao" column="IDCondicao" class="Condicao" lazy="false"/>
    <many-to-one name="Vendedor" column="IDVendedor" class="Pessoa" lazy="false"/>
    <many-to-one name="TipoPedido" column="IDTipoPedido" class="TipoPedido" lazy="false"/>
    <property name="IDCategoriaPadrao" column="IDCategoriaPadrao" type="Int32" />
    <many-to-one name="LocalEstoque" column="IDLocalEstoque" class="LocalEstoque" lazy="false"/>
    <many-to-one name="TipoDocumento" column="IDTipoDocumento" class="TipoDocumento" lazy="false"/>
    <many-to-one name="TipoDocumentoReg" column="IDTipoDocumentoReg" class="TipoDocumento" lazy="false"/>
    <many-to-one name="Portador" column="IDPortador" class="Portador" lazy="false"/>
    <many-to-one name="PortadorReg" column="IDPortadorReg" class="Portador" lazy="false"/>
    <many-to-one name="Banco" column="IDBanco" class="Banco" lazy="false"/>
    <many-to-one name="Acrescimo" column="IDAcrescimo" class="Acrescimo" lazy="false"/>
    <many-to-one name="Historico" column="IDHistorico" class="Historico" lazy="false"/>
    <many-to-one name="PlanoConta" column="IDPlanoConta" class="PlanoConta" lazy="false"/>
    <many-to-one name="Remessa" column="IDRemessa" class="Remessa" lazy="false"/>
    <many-to-one name="RemessaReg" column="IDRemessaReg" class="Remessa" lazy="false"/>
    <property name="AssuntoEmailBoleto" column="AssuntoEmailBoleto" type="string" />
    <property name="MensagemEmailBoleto" column="MensagemEmailBoleto" type="string" />
    <property name="UrlBoletos" column="UrlBoletos" type="string" />
    <property name="BAgruparProdutos" column="bAgruparProdutos" type="bool" />
    <property name="BAtualizarInfAdicional" column="bAtualizarInfAdicional" type="bool" />
    <property name="StatusPedido" column="StatusPedido" type="string" />
    <property name="BEnviaEstoqueMagento" column="EnviaEstoqueMagento" type="bool" />
    <property name="BEnviaPrecoMagento" column="EnviaPrecoMagento" type="bool" />
  </class>
</hibernate-mapping>
