<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="PessoaEngenheiro" table="PessoaEngenheiro" lazy="false">

    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <many-to-one name="Pessoa" column="IDPessoa" class="Pessoa" lazy="false" cascade="all-delete-orphan"  />    
    <property name="CREA" column="CREA" type="string" />
  </class>
</hibernate-mapping>