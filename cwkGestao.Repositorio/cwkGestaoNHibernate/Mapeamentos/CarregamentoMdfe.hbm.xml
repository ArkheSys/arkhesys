<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="CarregamentoMdfe" table="CarregamentoMdfe">
		<id name="ID" column="ID">
			<generator class="identity" />
		</id>

		<many-to-one name="Mdfe" column="IDMdfe" class="Mdfe" lazy="false" />
		<many-to-one name="UFCarregamento" column="IDUnidadeFederativaCarregamento" class="UF" lazy="false" />
		<many-to-one name="CidadeCarregamento" column="IDMunicipioCarregamento" class="Cidade" lazy="false" />
	</class>
</hibernate-mapping>
