<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="CodigoImposto" table="CodigoImposto">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<property name="CodCST" column="CodCST" type="string" />
	<property name="Descricao" column="Descricao" type="string" />
	<many-to-one name="Imposto" column="IDImposto" class="Imposto" lazy="false"/>

  </class>
</hibernate-mapping>
