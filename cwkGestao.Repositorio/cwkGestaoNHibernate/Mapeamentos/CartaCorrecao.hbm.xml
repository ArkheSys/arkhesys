<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="CartaCorrecao" table="CartaCorrecao">
    
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <many-to-one name="Nota" column="IDNota" class="Nota" />
    <property name="Sequencia" column="Sequencia" type="Int32" />
    <property name="Data" column="Data" type="DateTime" />
    <property name="TextoCorrecao" column="TextoCorrecao" type="string" length="1000" />
    <property name="LogEnvio" column="LogEnvio" type="string" length="100" />
    <property name="LogRecibo" column="LogRecibo" type="string" length="100" />
    <property name="XmlLogEnvNFe" column="XmlLogEnvNFe" type="StringClob" />
    <property name="XmlLogRecNFe" column="XmlLogRecNFe" type="StringClob" />
    <property name="XmlDestinatarioNFe" column="XmlDestinatarioNFe" type="StringClob" />
    <property name="UltimoXmlRecebido" column="UltimoXmlRecebido" type="StringClob" />
    <property name="DtEnvio" column="DtEnvio" type="DateTime" />
    <property name="Status" column="Status" type="string" length="10" />
    <property name="IncData" column="IncData" type="DateTime" />
    <property name="IncUsuario" column="IncUsuario" type="string" length="20" />
    <property name="AltData" column="AltData" type="DateTime" />
    <property name="AltUsuario" column="AltUsuario" type="string" length="20" />
    <property name="cStat" column="cStat" type="string" length="50" />
    <property name="Protocolo" column="Protocolo" type="string" length="50" />
  </class>
</hibernate-mapping>