<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="PessoaContabilista" table="PessoaContabilista">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<many-to-one name="Pessoa" column="IDPessoa" class="Pessoa" />
	<property name="CRC" column="CRC" type="string" />
	<property name="CNPJEscritorio" column="CNPJEscritorio" type="string" />

  </class>
</hibernate-mapping>
