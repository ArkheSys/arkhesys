<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="Ofd_Obra" table="Ofd_Obra">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<property name="Codigo" column="Codigo" type="Int32" />
	<property name="Descricao" column="Descricao" type="string" />
	<many-to-one name="Construtora" column="IDConstrutora" class="Pessoa" />
	<property name="ResponsavelNaObra" column="ResponsavelNaObra" type="string" />
	<property name="TelefoneResponsavelNaObra" column="TelefoneResponsavelNaObra" type="string" />
	<property name="Status" column="Status" type="Int32" />
  <property name="IncData" column="IncData" type="DateTime" />
  <property name="IncUsuario" column="IncUsuario" type="string" length="20" />
  <property name="AltData" column="AltData" type="DateTime" />
  <property name="AltUsuario" column="AltUsuario" type="string" length="20" />
  <bag name="Diarios" table="Ofd_ObraDiario" inverse="true" cascade="all-delete-orphan" >
    <key column="IDObra" />
    <one-to-many class="Ofd_ObraDiario" />
  </bag>
  <bag name="Arquivos" table="Ofd_ObraArquivos" inverse="true" cascade="all-delete-orphan" >
      <key column="IDObra" />
      <one-to-many class="Ofd_ObraArquivos" />
  </bag>
  <bag name="Agendamentos" table="Ofd_AgendamentoMaquina" inverse="true" cascade="all-delete-orphan" >
    <key column="IDOfd_Obra" />
    <one-to-many class="Ofd_AgendamentoMaquina" />
  </bag>
  </class>
</hibernate-mapping>
