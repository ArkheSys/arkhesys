<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="NotaImpostoServico" table="NotaImpostoServico">

    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <many-to-one name="Nota" column="IDNota" class="Nota" />
    <!--<many-to-one name="Servico" column="IDServico" class="Servico" lazy="false" />-->
    <many-to-one name="Imposto" column="IDServicoImposto" class="ServicoImposto" lazy="false" />
    <property name="Valor" column="Valor" type="decimal" precision="18" scale ="2"/>
    <property name="Aliquota" column="Aliquota" type="decimal" precision="18" scale="4"/>
    <property name="BaseCalculo" column="BaseCalculo" type="decimal" precision="18" scale="2"/>
    
    <property name="BaseRetencaoImposto" column="BaseRetencaoImposto" type="bool" />    
  </class>
</hibernate-mapping>
