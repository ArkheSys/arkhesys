<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="FichaTecnicaPCPItem" table="PCP_ItemFichaTecnica">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>

    <many-to-one name="FichaTecnicaPCP" column="IdFichaTecnicaPCP" class="FichaTecnicaPCP" not-null="true" lazy="false"/>
    <many-to-one name="FichaTecnicaPCPFilha" column="IdFichaTecnicaPCPFilha" class="FichaTecnicaPCP" lazy="false"/>
    <many-to-one name="Produto" column="IdProduto" class="Produto" lazy="false" />
    <property name="Quantidade" column="Quantidade" type="decimal" />
	  <property name="Aproveitamento" column="Aproveitamento" type="decimal" />
    <property name="DataVigor" column="DataVigor" type="DateTime" />

  </class>
</hibernate-mapping>
