<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="Banco" table="Banco">

	<id name="ID" column="ID">
		<generator class="identity" /> 
	</id>
	<property name="Codigo" column="Codigo" type="Int32" />
	<property name="Nome" column="Nome" type="string" length="60" />
	<property name="Ordem" column="Ordem" type="string" length="5" />
	<property name="ArquivoDeLicenca" column="ArquivoDeLicenca" type="string" length="255" />
	<property name="CodigoAgencia" column="CodigoAgencia" type="string" length="10" />
	<property name="ContaCorrente" column="ContaCorrente" type="string" length="15" />
	<property name="CodigoCedente" column="CodigoCedente" type="string" length="20" />
	<property name="CaminhoImagens" column="CaminhoImagens" type="string" length="255" />
	<property name="LayoutBoleto" column="LayoutBoleto" type="string" length="100" />
	<property name="CaminhoLayoutBoleto" column="CaminhoLayoutBoleto" type="string" length="255" />
	<property name="InstrucoesCaixa" column="InstrucoesCaixa" type="string" length="255" />
	<property name="Logotipo" column="Logotipo" type="string" length="255" />
	<property name="LayoutRemessa" column="LayoutRemessa" type="string" length="100" />
	<property name="LayoutRetorno" column="LayoutRetorno" type="string" length="100" />
	<property name="LayoutBoletoEmail" column="LayoutBoletoEmail" type="string" length="100" />
	<property name="UtilizaFechamentoCaixa" column="UtilizaFechamentoCaixa" type="bool"/>

    <bag name="MovimentacaoCheque" table="MovimentacaoCheque"  inverse="true" cascade="all-delete-orphan" lazy ="false" >
      <key column="IDBanco" />
      <one-to-many class="MovimentacaoCheque" />
    </bag>
    <bag name="MovimentacaoChequeDestino" table="MovimentacaoCheque"  inverse="true" cascade="all-delete-orphan" lazy ="false" >
      <key column="IDBancoDestino" />
      <one-to-many class="MovimentacaoCheque" />
    </bag>

  </class>
</hibernate-mapping>
		