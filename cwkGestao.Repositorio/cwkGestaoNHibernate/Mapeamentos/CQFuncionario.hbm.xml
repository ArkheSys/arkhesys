<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="CQFuncionario" table="CQFuncionario">

	<id name="ID" column="ID">
		<generator class="identity" /> 
	</id>
	<property name="Codigo" column="Codigo" type="Int32" />
	<many-to-one name="ControleQualidade" column="IDControleQualidade" class="ControleQualidade" />
					<many-to-one name="Funcionario1" column="IDFuncionario1" class="Pessoa" />
					<many-to-one name="Funcionario2" column="IDFuncionario2" class="Pessoa" />
					<many-to-one name="Cor" column="IDCor" class="Cor" />
					<property name="QtdDefeito" column="QtdDefeito" type="decimal" />
</class>
</hibernate-mapping>
		