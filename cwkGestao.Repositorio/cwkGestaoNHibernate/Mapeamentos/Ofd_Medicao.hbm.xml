<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="Ofd_Medicao" table="Ofd_Medicao">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<property name="Data" column="Data" type="DateTime" />
	<property name="Numero" column="Numero" type="Int32" />
	<many-to-one name="Proposta" column="IDProposta" class="Ofd_Proposta" />
	<property name="Status" column="Status" type="Int32" />
	<property name="ValorServicos" column="ValorServicos" type="decimal" />
	<property name="ValorMobilizacao" column="ValorMobilizacao" type="decimal" />
	<property name="ValorProjeto" column="ValorProjeto" type="decimal" />
	<property name="ValorHoraParada" column="ValorHoraParada" type="decimal" />
	<property name="TotalMedicao" column="TotalMedicao" type="decimal" />
	<property name="IncData" column="IncData" type="DateTime" not-null="false"/>
	<property name="IncUsuario" column="IncUsuario" type="string" />
	<property name="AltData" column="AltData" type="DateTime" not-null="false"/>
	<property name="AltUsuario" column="AltUsuario" type="string" />
    
  <bag name="MedicaoServicos" table="Ofd_MedicaoServicos" inverse="true" cascade="all-delete-orphan" >
    <key column="IDOfd_Medicao" />
    <one-to-many class="Ofd_MedicaoServicos" />
  </bag>
  </class>
</hibernate-mapping>
