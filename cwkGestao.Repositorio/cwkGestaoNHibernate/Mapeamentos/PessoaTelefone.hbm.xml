<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="PessoaTelefone" table="PessoaTelefone">

	<id name="ID" column="ID">
		<generator class="identity" /> 
	</id>
	<many-to-one name="Pessoa" column="IDPessoa" class="Pessoa" />
					<property name="Sequencia" column="Sequencia" type="Int32" />
	<property name="Tipo" column="Tipo" type="string" length="15" />
	<property name="Numero" column="Numero" type="string" length="20" />
	<property name="Contato" column="Contato" type="string" length="60" />
	<property name="BPrincipal" column="bPrincipal" type="bool" />
  <property name="Operadora" column="Operadora" type="string" length="15" />
</class>
</hibernate-mapping>
		