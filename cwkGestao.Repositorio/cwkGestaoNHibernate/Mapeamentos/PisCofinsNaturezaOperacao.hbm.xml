<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="PisCofinsNaturezaOperacao" table="PisCofinsNaturezaOperacao">

    <id name="ID" column="ID">
      <generator class="identity" />
    </id>

    <property name="Codigo" column="Codigo" type="Int32" />
    <property name="Descricao" column="Descricao" type="string"  length="200"/>

    <many-to-one name="TabelaPIS" column="IDTabelaPIS" class="TabelaPIS" lazy ="false"/>
    <many-to-one name="TabelaCOFINS" column="IDTabelaCOFINS" class="TabelaCOFINS" lazy ="false"/>

    <bag name="NaturezaOperacao" table="NaturezaOperacao"  inverse="true" cascade="all-delete-orphan" lazy ="false" >
      <key column="IDPisCofinsNaturezaOperacao" />
      <one-to-many class="NaturezaOperacao" />
    </bag>
    
  </class>
</hibernate-mapping>