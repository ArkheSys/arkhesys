<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="Ofd_PropostaRevisaoPerfuracoes" table="Ofd_PropostaRevisaoPerfuracoes">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<property name="Revisao" column="Revisao" type="string" />
	<property name="Observacao" column="Observacao" type="string" />
	<many-to-one name="Ofd_Proposta" column="IDOfd_Proposta" class="Ofd_Proposta" />

    <bag name="Perfuracoes" table="Ofd_PropostaPerfuracoes" cascade="all-delete-orphan" lazy ="false">
      <key column="IDOfd_PropostaRevisaoPerfuracoes" />
      <one-to-many class="Ofd_PropostaPerfuracoes" />
    </bag>
    
  </class>
</hibernate-mapping>
