<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="Movimento" table="Movimento">

    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <property name="Codigo" column="Codigo" type="Int32" />
    <property name="Sequencia" column="Sequencia" type="Int32" />
    <property name="Origem" column="Origem" type="Int32" />
    <many-to-one name="Documento" column="IDDocumento" class="Documento" lazy="false"/>
    <many-to-one name="MovCaixa" column="IDMovCaixa" class="MovCaixa" />
    <property name="Dt" column="Dt" type="DateTime" />
    <property name="Operacao" column="Operacao" type="Int32" />
    <property name="Tipo" column="Tipo" type="Int32" />
    <many-to-one name="PlanoConta" column="IDPlanoConta" class="PlanoConta" />
    <many-to-one name="CentroCusto" column="IDCentroCusto" class="CentroCusto" />
    <many-to-one name="Historico" column="IDHistorico" class="Historico" />
    <property name="ComplementoHist" column="ComplementoHist" type="string" length="180" />
    <property name="Valor" column="Valor" type="decimal" />
    <property name="IncData" column="IncData" type="DateTime" />
    <property name="IncUsuario" column="IncUsuario" type="string" length="20" />
    <property name="AltData" column="AltData" type="DateTime" />
    <property name="AltUsuario" column="AltUsuario" type="string" length="20" />
    
    <bag name="Rateio_Movs" table="Rateio_Mov" cascade="all-delete-orphan" lazy="false">
      <key column="IDMovimento" />
      <one-to-many class="Rateio_Mov" />
    </bag>

    <bag name="FormaPagamentoMovimentos" table="FormaPgtoMovimento" cascade="all" lazy="false">
      <key column="IDMovimento" />
      <one-to-many class="FormaPgtoMovimento" />
    </bag>
    
  </class>
</hibernate-mapping>
