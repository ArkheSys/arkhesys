<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="ContratoControle" table="ContratoControle">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<many-to-one name="Contrato" column="IDContrato" class="Contrato" />
	<property name="MesGerado" column="MesGerado" type="DateTime" />
  <property name="bServico" column="bServico" type="bool" />
  <many-to-one name="ContratoServico" column="idContratoServico" class="ContratoServico" />
  <many-to-one name="ContratoProduto" column="idContratoProduto" class="ContratoProduto" />
    
  <many-to-one name="NotaItem" column="idNotaItem" class="NotaItem" lazy="false" />
  <many-to-one name="NotaItemServico" column="idNotaItemServico" class="NotaItemServico" lazy="false" />
    
  </class>
</hibernate-mapping>
