<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="TabelaComissao" table="TabelaComissao">

	<id name="ID" column="ID">
		<generator class="identity" /> 
	</id>
	<property name="Codigo" column="Codigo" type="Int32" />
	<property name="Nome" column="Nome" type="string" length="60" />
	<property name="ValidadeInicial" column="ValidadeInicial" type="DateTime" />
	<property name="ValidadeFinal" column="ValidadeFinal" type="DateTime" />
</class>
</hibernate-mapping>
		