<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="Atendimento" table="Atendimento">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<property name="IncData" column="IncData" type="DateTime" />
	<property name="AltData" column="AltData" type="DateTime" />
	<property name="IncUsuario" column="IncUsuario" type="string" />
	<property name="AltUsuario" column="AltUsuario" type="string" />
	<property name="DtAtendimento" column="DtAtendimento" type="DateTime" />
	<property name="Entrada_1" column="Entrada_1" type="TimeAsTimeSpan" />
	<property name="Saida_1" column="Saida_1" type="TimeAsTimeSpan" />
	<property name="Entrada_2" column="Entrada_2" type="TimeAsTimeSpan" />
	<property name="Saida_2" column="Saida_2" type="TimeAsTimeSpan" />
  <property name="TotalHoras" column="TotalHoras" type="TimeAsTimeSpan" />
  <property name="TotalHorasFat" column="TotalHorasFat" type="TimeAsTimeSpan" />
	<property name="Descricao" column="Descricao" type="string" length="1000" />
	<many-to-one name="Cliente" column="IDPessoa" class="Pessoa" />
	<property name="DtConferencia" column="DtConferencia" type="DateTime" />
	<property name="Faturar" column="BFaturar" type="bool" />
	<property name="Observacoes" column="Observacoes" type="string" length="1000" />
	<many-to-one name="Nota" column="IDNota" class="Nota" lazy="false"/>
  <many-to-one name="Analista" column="IDCw_usuario" class="Cw_usuario" lazy="false"/>
  <many-to-one name="Status" column="IDStatusAtendimento" class="StatusAtendimento" lazy="false"/>
  <many-to-one name="FatAtendimento" column="IDFaturamentoAtendimento" class="FaturamentoAtendimento" lazy="false"/>
    

  </class>
</hibernate-mapping>
