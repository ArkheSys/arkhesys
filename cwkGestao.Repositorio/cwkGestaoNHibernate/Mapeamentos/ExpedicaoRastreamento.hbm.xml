<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="ExpedicaoRastreamento" table="ExpedicaoRastreamento">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<many-to-one name="Pedido" column="IDPedido" class="Pedido" lazy="false" />
	<many-to-one name="IDRastreamento" column="IDRastreamento" class="RastreamentoCorreio" lazy="false"/>
  <property name="PesoBruto" column="PesoBruto" type="decimal"/>
  <property name="PesoLiquido" column="PesoLiquido" type="decimal"/>
  <property name="bRastroCompleto" column="bRastroCompleto" type="bool"/>
  <many-to-one name="ServicoEnvioCorreio" column="IDServicoEnvioCorreio" class="ServicoEnvioCorreio" lazy="false"/>
  <property name="Tipo" column="Tipo" type="string"/>
  <property name="Status" column="Status" type="string"/>
  </class>
</hibernate-mapping>
