<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="UF" table="UF">

    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <property name="Codigo" column="Codigo" type="Int32" />
    <property name="Sigla" column="Sigla" type="string" length="2" />
    <property name="Nome" column="Nome" type="string" length="60" />
    <many-to-one name="Pais" column="IDPais" class="Pais" lazy="false" />
    <property name="IBGE" column="IBGE" type="string" length="20" />

  </class>
</hibernate-mapping>
