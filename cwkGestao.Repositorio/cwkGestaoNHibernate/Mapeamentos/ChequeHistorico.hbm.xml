<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="ChequeHistorico" table="ChequeHistorico">

    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <property name="Status" column="Status" type="Int32" />
    <property name="Data" column="Data" type="DateTime" />
    <property name="Observacao" column="Observacao" type="string" />
    <property name="Movimento" column="Movimento" type="string" />
    <property name="OrigemMovimento" column="OrigemMovimento" type="string" />
    
    <many-to-one name="Cheque" column="IDCheque" class="Cheque" lazy="false"/>
    <many-to-one name="Documento" column="IDDocumento" class="Documento" lazy="false"/>
    <many-to-one name="Historico" column="IDHistorico" class="Historico" lazy="false" />
    <many-to-one name="MovCaixa" column="IDMovCaixa" class="MovCaixa" lazy="false" />
  </class>
</hibernate-mapping>