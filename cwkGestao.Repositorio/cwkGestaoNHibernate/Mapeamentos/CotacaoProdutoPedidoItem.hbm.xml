	<class name="CotacaoProdutoPedidoItem" table="CotacaoProdutoPedidoItem">

	<id name="ID" column="ID">
		<generator class="identity" /> 
	</id>
	<property name="Codigo" column="Codigo" type="Int32" />
	<many-to-one name="CotacaoProduto" column="IDCotacaoProduto" class="CotacaoProduto" />
					<many-to-one name="PedidoItem" column="IDPedidoItem" class="PedidoItem" />
