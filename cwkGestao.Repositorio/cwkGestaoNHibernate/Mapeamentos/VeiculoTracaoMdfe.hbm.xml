<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="VeiculoTracaoMdfe" table="VeiculoTracaoMdfe">
		<id name="ID" column="ID">
			<generator class="identity" />
		</id>
		
		<many-to-one name="ProprietarioVeiculoMdfe" column="IDProprietarioVeiculoMdfe" class="ProprietarioVeiculoMdfe" lazy="false" />
		<many-to-one name="Condutor" column="IDCondutor" class="Condutor" lazy="false" />
		<many-to-one name="Veiculo" column="IDVeiculo" class="Veiculo" lazy="false" />
		<many-to-one name="Mdfe" column="IDMdfe" class="Mdfe" lazy="false" />
	</class>
</hibernate-mapping>
