<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="OrdemProducaoHistorico" table="OrdemProducaoHistorico">

	<id name="ID" column="ID">
		<generator class="identity" /> 
	</id>
	<property name="Sequencia" column="Sequencia" type="Int32" />
	<property name="Data" column="Data" type="DateTime" />
	<many-to-one name="OrdemProducao" column="IdOrdemProducao" class="OrdemProducao" />
					<many-to-one name="Classificacao" column="IdClassificacao" class="Classificacao" />
				</class>
</hibernate-mapping>
		