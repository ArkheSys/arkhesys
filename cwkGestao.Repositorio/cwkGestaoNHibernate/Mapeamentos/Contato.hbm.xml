<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="Contato" table="Contato">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<many-to-one name="Pessoa" column="IDPessoa" class="Pessoa" lazy="false"/>
	<property name="NomeContato" column="Contato" type="string" />
	<property name="FoneContato" column="FoneContato" type="string" />
	<many-to-one name="Atendente" column="IDCw_usuario" class="Cw_usuario" lazy="false"/>
	<property name="IncData" column="IncData" type="DateTime" />
	<property name="AltData" column="AltData" type="DateTime" />
	<property name="IncUsuario" column="IncUsuario" type="string" />
	<property name="AltUsuario" column="AltUsuario" type="string" />
    
  <bag name="ContatoDetalhes" table="ContatoDetalhe" inverse="true" cascade="all-delete-orphan" lazy="false">
    <key column="IDContato" />
    <one-to-many class="ContatoDetalhe" />
  </bag>
  </class>
</hibernate-mapping>
