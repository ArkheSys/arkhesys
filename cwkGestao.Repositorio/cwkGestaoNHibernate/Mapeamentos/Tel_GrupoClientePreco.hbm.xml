<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="Tel_GrupoClientePreco" table="Tel_GrupoClientePreco">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
        
    <property name="Area_Inicial" column="Area_Inicial" type="Int16" />
    <property name="Area_Final" column="Area_Final" type="Int16" />
    <property name="Prefixo_Inicial" column="Prefixo_Inicial" type="Int32" />
    <property name="Prefixo_Final" column="Prefixo_Final" type="Int32" />
    <property name="Range_Inicial" column="Range_Inicial" type="Int32" />
    <property name="Range_Final" column="Range_Final" type="Int32" />
    <property name="BDentro" column="bDentro" type="Boolean" />
    <property name="BFora" column="bFora" type="Boolean" />
    <property name="Valor" column="Valor" type="Decimal" />
    <many-to-one name="GrupoCliente" column="IdTel_GrupoCliente" class="Tel_GrupoCliente" />
    <many-to-one name="TipoLigacao" column="IdTel_TipoLigacao" class="Tel_TipoLigacao" lazy="false" />
    <property name="IncData" column="IncData" type="DateTime" />
    <property name="IncUsuario" column="IncUsuario" type="string" length="20" />
    <property name="AltData" column="AltData" type="DateTime" />
    <property name="AltUsuario" column="AltUsuario" type="string" length="20" />
    
  </class>
</hibernate-mapping>
