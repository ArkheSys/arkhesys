<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="RotaDetalhe" table="RotaDetalhe">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<many-to-one name="Rota" column="IdRota" class="Rota" />
	<property name="Codigo" column="Codigo" type="Int32" />
	<many-to-one name="Cliente" column="IdCliente" class="Pessoa" />
	<property name="Sequencia" column="Sequencia" type="Int32" />
	<property name="Situacao" column="Situacao" type="Int32" />
	<property name="Data" column="Data" type="DateTime" />
	<property name="Observacao" column="Observacao" type="string" />
  <many-to-one name="Operador" column="IdOperador" class="Pessoa"/>
    
  </class>
</hibernate-mapping>
