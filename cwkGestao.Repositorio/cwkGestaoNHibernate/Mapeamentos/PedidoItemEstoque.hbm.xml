<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="PedidoItemEstoque" table="PedidoItemEstoque">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>

    <many-to-one name="PedidoItem" column="IDPedidoItem" class="PedidoItem" not-null="true" lazy = "false"/>
    <property name="CMVUnit" column="CMVUnit" type="decimal" precision="18" scale="4" />
    <property name="Quantidade" column="Quantidade" type="decimal" precision="18" scale="4" />
    <property name="IncData" column="IncData" type="DateTime" />
    <property name="IncUsuario" column="IncUsuario" type="string" />
    <property name="AltData" column="AltData" type="DateTime" />
    <property name="AltUsuario" column="AltUsuario" type="string" />
    <many-to-one name="Identificador" column="IDIdentificador" class="Identificador" lazy = "false"/>

  </class>
</hibernate-mapping>
