<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="cepbr_bairro" table="cepbr_bairro">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<property name="bairro" column="bairro" type="string" />
	<many-to-one name="id_cidade" column="id_cidade" class="cepbr_cidade" />

  </class>
</hibernate-mapping>
