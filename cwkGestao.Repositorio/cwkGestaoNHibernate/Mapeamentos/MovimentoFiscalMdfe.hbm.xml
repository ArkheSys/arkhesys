<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="MovimentoFiscalMdfe" table="MovimentoFiscalMdfe">
		<id name="ID" column="ID">
			<generator class="identity" />
		</id>

		<property name="Serie" column="Serie" type="Int32" />
		<property name="XmlEnvio" column="XmlEnvio" type="string" />
		<property name="XmlRetorno" column="XmlRetorno" type="string" />
		<property name="Emitida" column="Emitida" type="Int32" />
		<property name="Cancelada" column="Cancelada" type="Int32" />
		<property name="DataEmissao" column="DataEmissao" type="DateTime" />
		<property name="Chave" column="Chave" type="string" />
		<property name="Cstat" column="Cstat" type="Int32" />
		<property name="Motivo" column="Motivo" type="string" />
		<property name="Recebimento" column="Recebimento" type="DateTime" />
		<property name="Ambiente" column="Ambiente" type="Int32" />
		<property name="Numero" column="Numero" type="Int32" />
		<property name="TipoDocumento" column="TipoDocumento" type="Int32" />
		<property name="Encerrada" column="Encerrada" type="Int32" />

		<many-to-one name="Mdfe" column="IDMdfe" class="Mdfe" lazy="false" />
	</class>
</hibernate-mapping>
