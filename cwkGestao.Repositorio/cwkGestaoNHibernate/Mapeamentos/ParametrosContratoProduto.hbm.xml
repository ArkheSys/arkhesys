<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="ParametrosContratoProduto" table="ParametrosContratoProduto">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
  
  <property name="Codigo" column="Codigo" type="Int32" />
  <property name="Descricao" column="Descricao" type="string" length="300" />
	<many-to-one name="Filial" column="IDFilial" class="Filial" lazy="false" />
	<many-to-one name="TipoNota" column="IDTipoNota" class="TipoNota" lazy="false" />
  <many-to-one name="TipoNotaServico" column="IDTipoNotaServico" class="TipoNota" lazy="false" />
	<many-to-one name="Condicao" column="IDCondicao" class="Condicao" lazy="false" />
	<many-to-one name="TipoPedido" column="IDTipoPedido" class="TipoPedido" lazy="false" />
	<many-to-one name="TabelaPreco" column="IDTabelaPreco" class="TabelaPreco" lazy="false" />
  <many-to-one name="Vendedor" column="IDVendedor" class="Pessoa" lazy="false" />
  <many-to-one name="Equipamento" column="IDOSEquipamento" class="OSEquipamento" lazy="false" />

  </class>
</hibernate-mapping>
