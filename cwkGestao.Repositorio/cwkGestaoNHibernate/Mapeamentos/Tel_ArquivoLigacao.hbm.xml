<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="Tel_ArquivoLigacao" table="Tel_ArquivoLigacao">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>

    <property name="Codigo" column="Codigo" type="Int32" />
    <property name="TipoArquivo" column="TipoArquivo" type="Int32" />
    <property name="DataImportacao" column="DataImportacao" type="DateTime" />
    <property name="CaminhoArquivo" column="CaminhoArquivo" type="string" />
    <property name="IncData" column="IncData" type="DateTime" />
    <property name="IncUsuario" column="IncUsuario" type="string" />
    <property name="AltData" column="AltData" type="DateTime" />
    <property name="AltUsuario" column="AltUsuario" type="string" />
    <property name="Referencia" column="Referencia" type="DateTime" />

    <bag name="Ligacoes" table="Tel_Ligacao" inverse="true" cascade="all-delete-orphan" >
      <key column="IdTel_ArquivoLigacao" />
      <one-to-many class="Tel_Ligacao" />
    </bag>

  </class>
</hibernate-mapping>
