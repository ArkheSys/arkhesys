<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="PessoaFuncionario" table="PessoaFuncionario">

	<id name="ID" column="ID">
		<generator class="identity" /> 
	</id>
  
    <many-to-one name="Pessoa" column="IDPessoa" class="Pessoa"  />
    <many-to-one name="CwUsuario" column="IDCwUsuario" class="Cw_usuario" lazy="false" />
    <property name="NumCarteiraTrab" column="NumCarteiraTrab" type="string" length="20" />

</class>
</hibernate-mapping>
		