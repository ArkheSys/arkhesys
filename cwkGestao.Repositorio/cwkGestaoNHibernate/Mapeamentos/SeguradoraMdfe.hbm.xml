<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="SeguradoraMdfe" table="SeguradoraMdfe">
		<id name="ID" column="ID">
			<generator class="identity" />
		</id>

		<property name="NumeroApolice" column="NumeroApolice" type="string" />

		<many-to-one name="Seguradora" column="IDSeguradora" class="Seguradora" lazy="false" />
		<many-to-one name="ResponsavelSeguroCargaMdfe" column="IDResponsavelSeguroCargaMdfe" class="ResponsavelSeguroCargaMdfe" lazy="false" />
		<many-to-one name="Mdfe" column="IDMdfe" class="Mdfe" lazy="false" />
	</class>
</hibernate-mapping>
