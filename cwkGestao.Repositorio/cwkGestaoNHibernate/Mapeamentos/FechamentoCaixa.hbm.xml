<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="FechamentoCaixa" table="FechamentoCaixa">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<many-to-one name="Banco" column="IDBanco" class="Banco" />
	<property name="DtFechamento" column="DtFechamento" type="DateTime" />
	<property name="UsuarioFechamento" column="UsuarioFechamento" type="string" />
	<property name="DtConciliacao" column="DtConciliacao" type="DateTime" />
  <property name="bFechado" column="bFechado" type="bool"/>
  <property name="ValorEntrada" column="ValorEntrada" type="decimal"/>
  <property name="ValorSaida" column="ValorSaida" type="decimal"/>
  <property name="Saldo" column="Saldo" type="decimal"/>
  <property name="SaldoInicial" column="SaldoInicial" type="decimal"/>

  </class>
</hibernate-mapping>
