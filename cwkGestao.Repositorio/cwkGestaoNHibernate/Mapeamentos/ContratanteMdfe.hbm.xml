<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="ContratanteMdfe" table="ContratanteMdfe">
		<id name="ID" column="ID">
			<generator class="identity" />
		</id>

		<many-to-one name="Mdfe" column="IDMdfe" class="Mdfe" lazy="false" />
		<property name="Cpf" column="CPF" type="string" />
		<property name="Cnpj" column="CNPJ" type="string" />
	</class>
</hibernate-mapping>
