<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="MercadoLivreConfiguracao" table="MercadoLivreConfiguracao">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	<many-to-one name="Fornecedor" column="IDFornecedor" class="Pessoa" />
	<many-to-one name="TabelaPreco" column="IDTabelaPreco" class="TabelaPreco" />
	<many-to-one name="Condicao" column="IDCondicao" class="Condicao" />
	<many-to-one name="Vendedor" column="IDVendedor" class="Pessoa" />
	<many-to-one name="LocalEstoque" column="IDLocalEstoque" class="LocalEstoque" />
	<property name="Aplicacao" column="IDAplicacao" type="long" />
	<property name="ChaveSecreta" column="ChaveSecreta" type="string" />
  <property name="Token" column="Token" type="string" />
  <property name="RefreshToken" column="RefreshToken" type="string" />
  <property name="URL" column="URL" type="string" />
  <property name="TempoSinc" column="TempoSinc" type="int" />
  <property name="IDUsuarioMercadoLivre" column="IDUsuarioMercadoLivre" type="int"/>
  <property name="DtInicioImportacao" column="DtInicioImportacao" type="DateTime"/>

    <many-to-one name="Filial" column="IDFilial" class="Filial" />
	<many-to-one name="TipoOrcamento" column="IDTipoOrcamento" class="TipoPedido" />

  </class>
</hibernate-mapping>
