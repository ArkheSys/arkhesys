<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
<class name="Impressora" table="Impressora">
	<id name="ID" column="ID">
		<generator class="identity" /> 
	</id>
	<property name="Codigo" column="Codigo" type="Int32" />
	<property name="Nome" column="Nome" type="string" length="60" />
	<property name="Caminho" column="Caminho" type="string" length="100" />
	<property name="Tipo" column="Tipo" type="bool" />
  <property name="TipoImpressao" column="TipoImpressao" />
</class>
</hibernate-mapping>
		