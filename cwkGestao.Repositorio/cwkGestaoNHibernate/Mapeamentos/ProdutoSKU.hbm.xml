<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="ProdutoSKU" table="ProdutoSKU">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<many-to-one name="Produto" column="IDProduto" class="Produto" />
	<property name="SKU" column="SKU" type="string" />

  </class>
</hibernate-mapping>
