<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="NotaServicoItem" table="NotaServicoItem">

    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <many-to-one name="NotaServico" column="IDNota" class="NotaServico" />
    <many-to-one name="Servico" column="IDServico" class="Servico" lazy="false" />
    <property name="Sequencia" column="Sequencia" type="Int32" />    
    <property name="ServicoDescricao" column="ServicoDescricao" type="string" length="100" />    
    <property name="Quantidade" column="Quantidade" type="decimal" precision="10" scale="2"/>    
    <property name="ValorUnitario" column="ValorUnitario" type="decimal" precision="18" scale ="2"/>
    <property name="Total" column="Total" type="decimal" precision="18" scale="2"/>    
  </class>
</hibernate-mapping>
