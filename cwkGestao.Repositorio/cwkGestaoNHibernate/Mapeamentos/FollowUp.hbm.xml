<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="FollowUp" table="FollowUp">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <property name="Data" column="Data" type="DateTime" />
    <property name="Descricao" column="Descricao" type="string" length="500" />
    <property name="Usuario" column="Usuario" type="string" length="50" />
    <property name="IDPedido" column="IDPedido" type="Int32" />
  </class>
</hibernate-mapping>