<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="ProdutoMercadoLivre" table="ProdutoMercadoLivre">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<many-to-one name="Produto" column="IDProduto" class="Produto" lazy="false"/>
	<property name="MercadoLivre" column="IDMercadoLivre" type="Int32" />
	<property name="Descricao" column="Descricao" type="string" />
	<property name="bAtivo" column="bAtivo" type="bool" />

  </class>
</hibernate-mapping>
