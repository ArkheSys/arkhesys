<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="Ofd_AgendamentoMaquinaOcorrencia" table="Ofd_AgendamentoMaquinaOcorrencia">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<many-to-one name="Ocorrencia" column="IDOcorrencia" class="Ofd_Ocorrencias" lazy="false"/>
	<many-to-one name="Ofd_AgendamentoMaquina" column="IDOfd_AgendamentoMaquina" class="Ofd_AgendamentoMaquina" lazy="false" />
	<property name="Observacao" column="Observacao" type="string" />
	<property name="TempoOcorrencia" column="TempoOcorrencia" type="DateTime" />
	<property name="IncData" column="IncData" type="DateTime" />
	<property name="AltData" column="AltData" type="DateTime" />
	<property name="IncUsuario" column="IncUsuario" type="string" />
	<property name="AltUsuario" column="AltUsuario" type="string" />

  </class>
</hibernate-mapping>
