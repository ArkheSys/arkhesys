<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="NotaItemServicoPedidoItemServico" table="NotaItemServicoPedidoItemServico">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<many-to-one name="PedidoItemServico" column="IDPedidoItemServico" class="PedidoItemServico" />
	<many-to-one name="NotaItemServico" column="IDNotaItemServico" class="NotaItemServico" />
	<property name="IncData" column="IncData" type="DateTime" />
	<property name="AltData" column="AltData" type="DateTime" />
	<property name="IncUsuario" column="IncUsuario" type="string" length="512" />
	<property name="AltUsuario" column="AltUsuario" type="string" length="512" />

  </class>
</hibernate-mapping>
