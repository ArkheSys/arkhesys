<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="PromocaoProduto" table="PromocaoProduto">

		<id name="ID" column="ID">
			<generator class="identity" />
		</id>

		<property name="PrecoBase" column="PrecoBase" type="decimal" />
		<property name="PrecoPromocao" column="PrecoPromocao" type="decimal" />

		<many-to-one name="Promocao" column="IDPromocao" class="Promocao" lazy="false"/>
		<many-to-one name="Produto" column="IDProduto" class="Produto" lazy="false"/>
	</class>
</hibernate-mapping>
