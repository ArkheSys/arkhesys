<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="ProprietarioVeiculoMdfe" table="ProprietarioVeiculoMdfe">
		<id name="ID" column="ID">
			<generator class="identity" />
		</id>

		<property name="Cnpj" column="CNPJ" type="string" />
		<property name="Cpf" column="CPF" type="string" />
		<property name="Rntrc" column="RNTRC" type="string" />
		<property name="Nome" column="Nome" type="string" />
		<property name="InscricaoEstadual" column="InscricaoEstadual" type="string" />
		<property name="TipoProprietario" column="TipoProprietario" type="int" />
		<property name="CodigoAgenciaPorto" column="CodigoAgenciaPorto" type="string" />

		<many-to-one name="Uf" column="IDUf" class="UF" lazy="false" />
	</class>
</hibernate-mapping>
