<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="LogAlteracaoPedidoItem" table="LogAlteracaoPedidoItem">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	<property name="Descricao" column="Descricao" type="string" />
	<property name="DescricaoAnterior" column="DescricaoAnterior" type="string" />
	<property name="Codigo" column="Codigo" type="string" />
	<property name="CodigoAnterior" column="CodigoAnterior" type="string" />
  <property name="DataInicialDesc" column="DataInicialDesc" type="DateTime" />
  <property name="DataFinalDesc" column="DataFinalDesc" type="DateTime" />
	<many-to-one name="Produto" column="IDProduto" class="Produto" />
	<many-to-one name="Servico" column="IDServico" class="Servico" />
  </class>
</hibernate-mapping>

