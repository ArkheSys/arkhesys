<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="Tel_GrupoCliente" table="Tel_GrupoCliente">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <property name="Codigo" column="Codigo" type="Int32" />
    <property name="Nome" column="Nome" type="string" />
    <property name="Endereco" column="Endereco" type="string" />
    <property name="Numero" column="Numero" type="string" />
    <property name="Complemento" column="Complemento" type="string" />
    <property name="Bairro" column="Bairro" type="string" />        
    <property name="CEP" column="CEP" type="string" />
    <property name="Contato" column="Contato" type="string" />
    <property name="Telefone" column="Telefone" type="string" />
    <many-to-one name="Cidade" column="IdCidade" class="Cidade" lazy="false"/>
    <property name="IncData" column="IncData" type="DateTime" />
    <property name="IncUsuario" column="IncUsuario" type="string" length="20" />
    <property name="AltData" column="AltData" type="DateTime" />
    <property name="AltUsuario" column="AltUsuario" type="string" length="20" />
    <many-to-one name="Acrescimo" column="IdAcrescimo" class="Acrescimo" />
    <bag name="Precos" table="Tel_GrupoClientePreco" inverse="true" cascade="all-delete-orphan" lazy="false" >
      <key column="IDTel_GrupoCliente" />
      <one-to-many class="Tel_GrupoClientePreco" />
    </bag>

    <bag name="Telefones" table="Tel_GrupoClienteTelefone" inverse="true" cascade="all-delete-orphan" lazy="false">
      <key column="IDTel_GrupoCliente" />
      <one-to-many class="Tel_GrupoClienteTelefone" />
    </bag>
    
  </class>
</hibernate-mapping>
