<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="ArquivoRemessa" table="ArquivoRemessa">

    <id name="ID" column="ID" unsaved-value="0">
      <generator class="identity" />
    </id>
    <property name="NumRemessa" column="NumRemessa" type="Int32" />
    <property name="DataCriacao" column="DataCriacao" type="DateTime" />
    <property name="UsuarioCriacao" column="UsuarioCriacao" type="string" length="200" />
    <property name="DataAlteracao" column="DataAlteracao" type="DateTime" not-null="false"/>
    <property name="UsuarioAlteracao" column="UsuarioAlteracao" type="string" length="60" />
    <many-to-one name="Filial" column="IdFilial" class="Filial" lazy="false"/>
    <many-to-one name="Remessa" column="IdRemessa" class="Remessa" lazy="false"/>
    <property name="tipoFiltro" column="tipoFiltro" type="Int32" />
    <property name="dataInicialFiltro" column="dataInicialFiltro" type="DateTime" />
    <property name="dataFinalFiltro" column="dataFinalFiltro" type="DateTime" />
  </class>
</hibernate-mapping>
