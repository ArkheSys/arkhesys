<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="ProdutoCodBarra" table="ProdutoCodBarra">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>	
	<many-to-one name="Produto" column="IdProduto" class="Produto" />
	<property name="BControleIdentificacao" column="bControleIdentificacao" type="bool" />
	<property name="CodigoBarra" column="CodigoBarra" type="string" />

  </class>
</hibernate-mapping>
