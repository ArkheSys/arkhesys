<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="ProdutoVendedorComissao" table="ProdutoVendedorComissao">

    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <many-to-one name="Produto" column="IDProduto" class="Produto"/>
    <many-to-one name="Vendedor" column="IDVendedor" class="PessoaVendedor" />
    <property name="ComissaoPct" column="ComissaoPct" type="decimal" />
    <property name="TipoComissao" column="TipoComissao" type="byte" />
    <property name="TipoComissaoProdutoServico" column="TipoComissaoProdutoServico" type="Int32" />
    <property name="ComissaoPctServico" column="ComissaoPctServico" type="decimal" />
    <property name="TipoComissaoServico" column="TipoComissaoServico" type="byte" />
    <property name="ComissaoPctGrupoEstoque" column="ComissaoPctGrupoEstoque" type="decimal" />
    <property name="TipoComissaoGrupoEstoque" column="TipoComissaoGrupoEstoque" type="byte" /> 
    <many-to-one name="Servico" column="IDServico" class="Servico" />
    <many-to-one name="GrupoEstoque" column="IDGrupoEstoque" class="GrupoEstoque" />

  </class>
</hibernate-mapping>
