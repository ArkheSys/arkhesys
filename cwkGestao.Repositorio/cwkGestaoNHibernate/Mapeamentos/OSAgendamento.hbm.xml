<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="OSAgendamento" table="OSAgendamento">

    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <many-to-one name="OSOrdemServico" column="IDOSOrdemServico" class="OSOrdemServico" lazy="false" />
    <many-to-one name="Funcionario" column="IDFuncionario" class="PessoaFuncionario" lazy="false" />
    <property name="Inicio" column="Inicio" type="DateTime" />
    <property name="Fim" column="Fim" type="DateTime" />
  </class>
</hibernate-mapping>