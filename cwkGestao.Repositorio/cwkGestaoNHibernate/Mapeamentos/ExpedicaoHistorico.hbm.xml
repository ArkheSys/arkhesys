<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="ExpedicaoHistorico" table="ExpedicaoHistorico">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<many-to-one name="ExpedicaoRastreamento" column="IDExpedicaoRastreamento" class="ExpedicaoRastreamento" lazy="false" />
	<many-to-one name="StatusEntEventoCorreio" column="IDStatusEntEventoCorreio" class="StatusEntEventoCorreio" lazy="false" />
	<property name="DataEvento" column="DataEvento" type="DateTime" />

  </class>
</hibernate-mapping>
