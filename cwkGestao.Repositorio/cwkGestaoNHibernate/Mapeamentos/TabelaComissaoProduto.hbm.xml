<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="TabelaComissaoProduto" table="TabelaComissaoProduto">

	<id name="ID" column="ID">
		<generator class="identity" /> 
	</id>
	<many-to-one name="TabelaComissao" column="IDTabelaComissao" class="TabelaComissao" />
					<property name="Codigo" column="Codigo" type="Int32" />
	<many-to-one name="Produto" column="IDProduto" class="Produto" />
					<property name="Comissao" column="Comissao" type="decimal" />
</class>
</hibernate-mapping>
		