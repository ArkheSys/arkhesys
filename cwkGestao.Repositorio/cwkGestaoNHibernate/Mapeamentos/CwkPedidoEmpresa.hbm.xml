<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="CwkPedidoEmpresa" table="CwkPedidoEmpresa">

    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <many-to-one name="Pedido" column="IDPedido" class="CwkPedido" />
    <property name="Seq" column="Seq" type="Int32" />
    <many-to-one name="ClienteRevenda" column="IDClienteRevenda" class="ClienteRevenda" lazy="false" />
    <property name="Observacao" column="Observacao" type="string" length="1000" />
    <property name="IncData" column="IncData" type="DateTime" />
    <property name="IncHora" column="IncHora" type="DateTime" />
    <property name="IncUsuario" column="IncUsuario" type="string" length="20" />
    <property name="AltData" column="AltData" type="DateTime" />
    <property name="AltHora" column="AltHora" type="DateTime" />
    <property name="AltUsuario" column="AltUsuario" type="string" length="20" />
    <many-to-one name="Servico" column="IDServico" class="Servico" lazy="false"/>
    <property name="Senha" column="Senha" type="string" length="100" />
    <property name="ContraSenha" column="ContraSenha" type="string" length="200" />
    <property name="TipoLicenca" column="TipoLicenca" type="Int32" />
    <property name="TipoServico" column="TipoServico" type="Int32" />
    <property name="Quantidade" column="Quantidade" type="Int32" />
    <property name="InfMaquina" column="InfMaquina" type="string" length="50" />
    <property name="ArquivoLicencaSerializado" type="string" >
      <column name="ArquivoLicencaSerializado" sql-type="NTEXT"/>
    </property>
    <property name="ValorProduto" column="ValorProduto" type="decimal" />
    <many-to-one name="Produto" column="IDProduto" class="Produto" lazy="false"/>
    <property name="NumeroSerie" column="NumeroSerie" type="Int32" />
  </class>
</hibernate-mapping>
