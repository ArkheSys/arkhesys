<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="ContratoServico" table="ContratoServico">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<many-to-one name="Contrato" column="IDContrato" class="Contrato" lazy="false" />
	<property name="Sequencia" column="Sequencia" type="Int32" />
	<many-to-one name="Servico" column="IDServico" class="Servico" lazy="false" />
	<property name="Quantidade" column="Quantidade" type="Int32" />
	<property name="Valor" column="Valor" type="decimal" />
	<property name="ValorDesconto" column="ValorDesconto" type="decimal" />
	<property name="PercDesconto" column="PercDesconto" type="decimal" />
	<property name="Total" column="Total" type="decimal" />
	<many-to-one name="PessoaServico" column="IDPessoa" class="Pessoa" lazy="false" />
	<property name="DataInicioFaturamento" column="DataInicioFaturamento" type="DateTime" />
	<property name="BJaneiro" column="bJaneiro" type="bool" />
	<property name="BFevereiro" column="bFevereiro" type="bool" />
	<property name="BMarco" column="bMarco" type="bool" />
	<property name="BAbril" column="bAbril" type="bool" />
	<property name="BMaio" column="bMaio" type="bool" />
	<property name="BJunho" column="bJunho" type="bool" />
	<property name="BJulho" column="bJulho" type="bool" />
	<property name="BAgosto" column="bAgosto" type="bool" />
	<property name="BSetembro" column="bSetembro" type="bool" />
	<property name="BOutubro" column="bOutubro" type="bool" />
	<property name="BNovembro" column="bNovembro" type="bool" />
	<property name="BDezembro" column="bDezembro" type="bool" />
	<property name="DataCancelamento" />
	<property name="MovitoCancelamento" column="MovitoCancelamento" type="string" />
	<property name="IncData" column="IncData" type="DateTime" />
	<property name="IncUsuario" column="IncUsuario" type="string" />
	<property name="AltData" column="AltData" type="DateTime" />
	<property name="AltUsuario" column="AltUsuario" type="string" />
  <property name="bDocumentosGerados" column="bDocumentosGerados" type="bool" />
  <property name="DiaVencimento" column="DiaVencimento" type="int"/>
    
  </class>
</hibernate-mapping>
