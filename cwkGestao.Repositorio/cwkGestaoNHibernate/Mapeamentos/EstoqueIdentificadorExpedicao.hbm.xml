<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="EstoqueIdentificadorExpedicao" table="EstoqueIdentificadorExpedicao">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    
	<many-to-one name="IDIdentificador" column="IDIdentificador" class="Identificador" lazy ="false"/>
	<property name="Saldo" column="Saldo" type="decimal" />
	<property name="CustoUnitario" column="CustoUnitario" type="decimal" />
  <property name="QuantidadeBaixada" column="QuantidadeBaixada" type="decimal" />
	<property name="SaldoDisponivel" column="SaldoDisponivel" type="decimal" />
  <property name="bGravado" column="bGravado" type="bool" />

  </class>
</hibernate-mapping>
