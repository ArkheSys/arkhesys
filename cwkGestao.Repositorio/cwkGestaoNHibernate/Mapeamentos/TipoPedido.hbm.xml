<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="TipoPedido" table="TipoPedido">

    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <property name="Codigo" column="Codigo" type="Int32" />
    <property name="Ordem" column="Ordem" type="string" length="5" />
    <property name="Nome" column="Nome" type="string" length="60" />
    <property name="BReservaEstoque" column="bReservaEstoque" type="bool" />
    <property name="BPermiteEstoqueNegativo" column="bPermiteEstqueNegativo" type="bool" />
    <property name="Ent_Sai" column="Ent_Sai" type="Int32" />
    <property name="Tipo" />
    <property name="TipoPreco" column="TipoPreco" type="Int32" />
    <property name="Observacao" column="Observacao" type="string" length="150" />
    <many-to-one name="TipoNota" column="IDTipoNota" class="TipoNota" lazy ="false"/>
    <many-to-one name="Impressora" column="IDImpressora" class="Impressora" />
    <many-to-one name="LocalEstoque" column="IDLocalEstoque" class="LocalEstoque" />
    <property name="BaseCalculoParcelas" column="BaseCalculoParcelas" />
    <property name="CalcularST" column="CalcularST" type="bool" />
    <property name="Truncar" column="Truncar" type="bool" />
    <property name="NomeRelatorio" column="NomeRelatorio" type="string" length="80" />
    <property name="BLimiteCredito" column="bLimiteCredito" type="bool" />
    <property name="bRecalcula" column="bRecalcula" type="int" />
    <property name="bRecalculaPMudTabPreco" column="bRecalculaPMudTabPreco" type="int" />
    <property name="Status" column="Status" type="int" />
    <property name="StatusPosAprovacao" column="StatusPosAprovacao" type="int" />
    <property name="bShlDistribuicao" column="bShlDistribuicao" type="bool"/>
    <property name="CodShl" column="CodShl" type="int"/>
    <property name="Categoria" column="Categoria" type="int"/>
    <property name="Referencias" column="Referencias" type="string"/>
  </class>
</hibernate-mapping>
