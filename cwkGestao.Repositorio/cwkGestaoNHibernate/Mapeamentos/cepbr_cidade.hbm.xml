<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="cepbr_cidade" table="cepbr_cidade">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	
	<property name="cidade" column="cidade" type="string" />
	<property name="uf" column="uf" type="string" />
	<property name="cod_ibge" column="cod_ibge" type="string" />
	<property name="area" column="area" type="" />
	<many-to-one name="CidadeCwork" column="IDCidadeCwork" class="Cidade"/>

  </class>
</hibernate-mapping>
