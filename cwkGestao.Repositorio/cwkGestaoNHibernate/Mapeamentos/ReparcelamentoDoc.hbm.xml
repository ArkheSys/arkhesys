<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="ReparcelamentoDoc" table="ReparcelamentoDoc">

    <id name="ID" column="Id">
      <generator class="identity" />
    </id>
    <many-to-one name="Reparcelamento" column="IdReparcelamento" class="Reparcelamento" />
    <property name="Tipo" column="Tipo" type="bool" />
    <many-to-one name="Documento" column="IdDocumento" class="Documento" cascade="save-update"  />
  </class>
</hibernate-mapping>
