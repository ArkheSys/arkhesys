<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="MovimentacaoCreditoPedido" table="MovimentacaoCreditoPedido">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <property name="IDCliente" column="IDCliente" type="Int32" />
    <property name="IDPedido" column="IDPedido" type="Int32" />
    <property name="IDFrenteCaixaNota" column="IDFrenteCaixaNota" type="Int32" />
    <property name="IDNota" column="IDNota" type="Int32" />
    <property name="IDMotivoDevolucao" column="IDMotivoDevolucao" type="Int32" />
    <property name="MotivoDevolucao" column="MotivoDevolucao" type="string" length="500" />
    <property name="Data" column="Data" type="DateTime" />
    <property name="Valor" column="Valor" type="decimal" />
    <property name="Saldo" column="Saldo" type="decimal" />
    <property name="Tipo" column="Tipo" type="Int32" not-null="false"/>
  </class>
</hibernate-mapping>