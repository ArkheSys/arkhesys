<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="TabelaCFOP" table="TabelaCFOP">

		<id name="ID" column="ID">
			<generator class="identity" />
		</id>
		<property name="CFOP" column="CFOP" type="string" length="4" />
		<property name="Nome" column="Nome" type="string" length="2000" />
		<property name="BCalcICMS" column="bCalcICMS" type="bool" />
		<property name="BDevolucao" column="bDevolucao" type="bool" />
		<property name="Codigo" column="Codigo" type="Int32" />
		<property name="CfopVinculadoEntrada" column="CfopVinculadoEntrada" type="string" length="4" />
		<many-to-one name="PlanoConta" column="IDPlanoConta" class="PlanoConta" />
	</class>
</hibernate-mapping>
