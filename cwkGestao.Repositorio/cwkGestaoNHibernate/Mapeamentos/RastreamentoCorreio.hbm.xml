<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="RastreamentoCorreio" table="RastreamentoCorreio">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	<many-to-one name="TipoServico" column="IDTipoServico" class="TipoServicoTransporte" lazy="false"/>
	<property name="Letra" column="Letra" type="string" />
	<property name="Numero" column="Numero" type="string" />
  <property name="CodigoEntregaMagento" column="CodigoEntregaMagento" type="string" />
  <property name="SolicitadoWScorreios" column="SolicitadoWScorreios" type="bool" />
  <property name="RastreamentoCompleto" column="RastreamentoCompleto" type="bool" />
  <many-to-one name="Filial" column="IDFilial" class="Filial"/>
  </class>
</hibernate-mapping>
