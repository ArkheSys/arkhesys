<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="ProdutoVarejo" table="ProdutoVarejo">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>    
    <property name="IDProduto" column="IDProduto" type="Int32" />
    <property name="QuantidadeInicial" column="QuantidadeInicial" type="Int32" />
    <property name="QuantidadeFinal" column="QuantidadeFinal" type="Int32" />
    <property name="Preco" column="Preco" type="decimal" />
  </class>
</hibernate-mapping>
