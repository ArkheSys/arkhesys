<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
  <class name="EtapaProducao" table="EtapaProducao">

    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <many-to-one name="Produto" column="IDProduto" class="Produto" />
    <many-to-one name="Servico" column="IDServico" class="Servico" />
    <property name="Sequencia" column="Sequencia" type="Int32" />
    <property name="Titulo" column="Titulo" type="string" length="160" />
    <property name="Observacao" column="Observacao" type="string" length="600" />
  </class>
</hibernate-mapping>
