<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cwkGestao.Modelo" namespace="cwkGestao.Modelo">
	<class name="PessoaVendedor" table="PessoaVendedor">
    <id name="ID" column="ID">
      <generator class="identity" /> 
    </id>
    <many-to-one name="Pessoa" column="IDPessoa" class="Pessoa" />
    <property name="ComissaoVendedor" column="ComissaoVendedor" type="decimal" />
    <property name="ComissaoServicoVendedor" column="ComissaoServicoVendedor" type="decimal" />
    <property name="Senha" column="Senha" type="string" length="10" />

    <bag name="ProdutoVendedorComissoes" table="ProdutoVendedorComissao" cascade="all-delete-orphan" inverse="true">
      <key column="IDVendedor" />
      <one-to-many class="ProdutoVendedorComissao" />
    </bag>
  </class>
</hibernate-mapping>
		